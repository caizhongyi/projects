.body{
    max-width:640px;
    margin:auto;
    min-height:100%;
    background-color:#e1e0de;
    position:relative;
}

.body{
    display:block;
    background:#e9e9e9;
    background:url(../images/v47_2.jpg);
}

/******************************/
.list_ul li{

}

.list_ul li{
    border-bottom:1px solid #ccc;
}

.list_ul li a{display:block;}
.list_ul li a>*{
    color:#707070;
    height:60px;
    vertical-align: middle;
}

.list_ul li dd div{
    width:60px;
    text-align:center;
}

.list_ul li dd img{
    max-width:45px;
    max-height:40px;
    margin:auto;
}

.list_ul li hgroup>*{
    line-height:20px;
    font-size:14px;
    font-weight:100;
    overflow: hidden;
    height: 20px;
    padding-right: 10px;
}
.list_ul li hgroup>h2{
    font-size:12px;
    color:#909090;
}