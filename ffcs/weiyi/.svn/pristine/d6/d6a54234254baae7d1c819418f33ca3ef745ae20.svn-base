body,h1,h2,h3,h4,h5,ul,ol,li,dl,dt,dd,p{margin: 0; padding: 0;}

table {
    border-collapse: collapse;
    border-spacing: 0;
}
h1,
h2,
h3,
h4,
h5,
h6 {
    font-size: 100%;
}
ul,
ol,
li {
    list-style: none;
}
em,
i {
    font-style: normal;
}
img {
    border: none;
}
img {
    vertical-align: middle;
}
body {
    color: #000;
    background: #F4F5F5; font: normal 14px/1.5em arial;
}
.clearfix:after {
    display: block;
    content: '';
    clear: both;
    visibility: hidden;
}
.none {
    display: none;
}
.no-spacing {
    margin: 0;
    padding: 0;
}
.margin-center {
    margin: 0 auto;
    text-align: center;
}

a,a:hover{text-decoration: none; color:#000;}
a,img{outline: none;}

b{font-weight: normal;}
i{font-style: normal;}

.clearfix:before, .clearfix:after { content:' '; display:block; overflow:hidden; visibility:hidden; width:0; height:0;}
*.clearfix:after {clear:both;}
.clearfix{zoom:1;}

/*头部导航*/
