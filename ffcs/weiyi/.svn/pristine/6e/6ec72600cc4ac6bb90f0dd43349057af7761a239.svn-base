.box_swipe ul li span {
    bottom: 5px;
    color: #FFFFFF;
    display: inline-block;
    left: 10px;
    position: absolute;
    width: 75%;
}
.box_swipe > ol {
    height: 20px;
    margin-top: -25px;
    padding-right: 15px;
    position: relative;
    text-align: center;
    z-index: 10;
}
.box_swipe > ol > li {
    background-color: #cdcdcd;
    border-radius: 8px;
    display: inline-block;
    height: 8px;
    margin: 5px 0;
    width: 8px;
}
.box_swipe > ol > li.on {
    background-color: rgba(235, 102, 25, 0.9);
}