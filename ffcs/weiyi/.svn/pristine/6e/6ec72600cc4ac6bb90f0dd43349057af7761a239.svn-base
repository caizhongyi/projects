.clip-lists li.clip-item {
    background-color: #FFFFFF;
    border: 1px solid #DDDDDD;
    border-radius: 3px;
    margin: 0 1px 5px;
    position: relative;
}
.clip-lists li.clip-item .refresh {
    background: url("../images/b5342c56.refresh.png") no-repeat scroll 15px center / 13px 16px rgba(0, 0, 0, 0);
    display: block;
    height: 40px;
    line-height: 40px;
    position: absolute;
    text-align: right;
    width: 80px;
    z-index: 10;
}
.clip-lists li.clip-item .title {
    cursor: pointer;
    height: 40px;
    line-height: 40px;
    padding-left: 85px;
    position: relative;
}
.clip-lists li.clip-item .title:after {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: #FFFFFF #9B9B9B #9B9B9B #FFFFFF;
    border-image: none;
    border-style: solid;
    border-width: 3px;
    content: "";
    display: block;
    height: 5px;
    position: absolute;
    right: 15px;
    top: 14px;
    transform: rotate(-45deg);
    transition: all 0.3s ease 0s;
    width: 5px;
}
.clip-lists li.clip-item .title.open:after {
    top: 10px;
    transform: rotate(45deg);
}
.clip-lists li.clip-item .name {
    color: #EB6619;
}
