@charset "utf-8";

/* CSS Document */
body,div,dl,dt,dd,ul,ol,li,pre,form,fieldset,input,textarea,p,blockquote,th,td,font,form,p,h1,h2,h3,h4,h5,h6 {padding:0; margin:0;}
input,select{font-size:12px;line-height:16px;}
html,body{ -webkit-text-size-adjust:none;}/*google浏览器字体兼容*/
img{border:0;}
ul,li{list-style-type:none;}
.tc{text-align:center;}
.tl{text-align:left;}
.tr{text-align:right;}
.dis{display:block;}
.undis{display:none;}
.house{ cursor:pointer;}
.fl{float:left;}
.fr{float:right;}
.cl{clear:both;}
.fixed{left:0;margin-top:0;top:0;width:100%;position:fixed;/*_position:absolute;_bottom:auto;_top:expression(eval(document.documentElement.scrollTop));*/z-index:999;}
.clearfix {
    *zoom: 1;
}
.clearfix:before,
.clearfix:after {
    display: table;
    line-height: 0;
    content: "";
}
.clearfix:after {
    clear: both;
}
