img{ max-width: 100%;}

label{ display: inline-block ;}
.zero-checkbox , .zero-radio{ display: inline-block; width: 10px; height: 10px;  margin: 0 5px; }
.zero-checkbox input,
.zero-radio input{opacity: 0; filter:~'alpah(opacity=0)';  width: 100%; height: 100%;}


.radio{ cursor: pointer; display: inline-block; }
.radio input{ cursor: pointer; }

.wrap{ padding: 10px;}
.vote h1{ font-size: 25px;}
.vote .summary{ font-size: 18px; margin:10px 0; }
.vote .date{ color: #999;}
.vote .content{ font-size: 16px;line-height: 1.2;}
.vote .content .img{ margin-bottom: 10px;}
.vote p{ line-height: 1.2;}

.vote-list{ margin-bottom: 50px;}
.vote-list .title{ font-family: "microsoft yahei"; font-size: 16px; margin-bottom: 10px; }
.vote-list li {  padding: 40px 5px; border-bottom: 1px solid #ddd;}
.vote-list li:nth-child(2n){ background: #f9f9f9;}
.vote-list .progress{  margin: 0; }
.vote-list .dovote{ background: url(../images/voteit.png) no-repeat; display: inline-block; width: 37px; height: 45px;  vertical-align: middle;}
.vote-list .dovote span{vertical-align: middle;}
.vote-list .dovote.checked{ background-position:  -37px 0;}
.vote-list .dovote.checked + span{ color: red;}

.titlebar{background: #fafafa; padding: 10px; border-top: 1px solid #ddd; border-bottom: 1px solid #ddd; margin: 20px 0; font-family: "microsoft yahei"}
.titlebar em { color: #a0a0a0; font-style: normal;}

.progress{ height: 15px;}
.progress-info .percent{ color: #5bb6de;}
.progress-success .percent{ color: #7bd23b;}
.progress-warning .percent{ color: #f38c1d;}
.progressbar .percent em{ color: #7d7d7d; font-style: normal;}
.progressbar .progress{  width: 80%; margin-right: 10px;}
.progressbar .percent{ font-size: 18px;}
.progressbar .progress,
.progressbar .percent { display: inline-block; *display:inline; zoom:1;  vertical-align: middle; }

.radio-round{ background: url(../images/radio.png) no-repeat -33px 0; width: 31px; height: 33px; display: inline-block; padding: 0; margin: 0 5px 0 0; vertical-align: middle;}
.radio-round.checked{ background-position: 2px -3px;}
.radio-round input{ margin: 0;}

.simg{ position: relative; display: block;}
.simg:after ,.simg:before {position: absolute; bottom:0px; color: #fff;font-size: 18px; height: 40px; line-height: 40px;  }
.simg:after{ content: '';  background-color:rgba(0,0,0,0.5); width: 100%; display: block;left: 0;}
.simg:before{ content: attr(title); z-index: 1;  right: 10px; padding: 0;}

.fixedbar-bottom{
    width: 100%;
    padding: 30px 0 20px 0;
    text-align: center;
    position: fixed;
    bottom: 0;
    left: 0;
    background-image: -webkit-linear-gradient(top,transparent 10% ,#4a4a4a 80%); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top,transparent 10% ,#4a4a4a 80%); /* FF3.6 */
    background-image: -ms-linear-gradient(top,transparent 10% ,#4a4a4a 80%);
    background-image: -o-linear-gradient(top,transparent 10% ,#4a4a4a 80%); /* Opera 11.10+ */
    background-image: linear-gradient(top,transparent 10% ,#4a4a4a 80%);

}


