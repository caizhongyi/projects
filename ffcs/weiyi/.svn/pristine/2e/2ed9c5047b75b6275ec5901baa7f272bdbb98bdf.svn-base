.wap_edit_area {
	position: absolute;
	z-index: 1;
	border: 2px solid #f00;
}
#wap_content {
	margin: 0 auto;
	margin-top: 36px;
}
.wap_icon {
	width: 100px;
	height: 120px;
	float: left;
	overflow: hidden;
}
.wap_icon a:hover {
	text-decoration: none;
}
.wap_icon img {
	display: block;
	width: 64px;
	height: 64px;
	border-radius: 10px;
	margin: 0 auto;
	box-shadow: 2px 2px 2px #ccc;
}
.wap_icon span {
	display: block;
	width: 80px;
	height: 20px;
	line-height: 20px;
	overflow: hidden;
	margin: 0 auto;
	text-align: center;
	font-weight: bold;
	color: #333;
	font-size: 12px;
	text-shadow: 0px 1px 2px #ccc;
}
.content_title {
	text-align: center;
	margin: 0;
	margin-top: 10px;
	padding: 0;
}
.content_time {
	color: #ccc;
	padding: 5px;
	padding-bottom: 0;
	text-align: center;
}
.content_pic {
	width: 46px;
	height: 46px;
	float: left;
	margin-right: 10px;
}

.style_1_icon {
	width: 50%;
	float: left;
}
.style_1_content {
	display: block;
	margin: 5px;
	border-radius: 5px;
	height: 110px;
	background: #fff;
	box-shadow: 0px 1px 3px #ccc;
	position: relative;
	overflow: hidden;
}
.style_1_img {
	border-radius: 5px 5px 0 0;
	height: 80px;
	overflow: hidden;
	background-repeat: no-repeat;
	background-position: center;
}
.style_1_img img {
	width: 100%;
	border-radius: 5px 5px 0 0;
}
.style_1_content span {
	display: block;
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	height: 30px;
	line-height: 30px;
	background: #fff;
	text-align: center;
	color: #000;
	overflow: hidden;
	border-radius: 0 0 5px 5px;
}

.style_2_icon {
	width: 50%;
	float: left;
}
.style_2_content {
	display: block;
	margin: 5px;
	border-radius: 10px;
	height: 175px;
	background: #fff;
	box-shadow: 0px 1px 3px #ccc;
	overflow: hidden;
	text-align: center;
	color: #000;
	font-size: 14px;
}
.style_2_img {
	width: 116px;
	height: 116px;
	margin: 8px auto;
	padding: 6px;
	border-radius: 64px;
	background: #f5f5f5;
	box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.11) inset;
	background-repeat: no-repeat;
	background-position: center;
}
.style_2_img img {
	width: 116px;
	height: 116px;
	border-radius: 58px;
}
.style_2_content span {
	display: block;
	height: 23px;
	line-height: 23px;
	overflow: hidden;
}

.style_3_icon {
	float: left;
}
.style_3_0_icon, .style_3_2_icon, .style_3_4_icon, .style_3_6_icon, .style_3_7_icon, .style_3_9_icon {
	width: 33%;
}
.style_3_3_icon, .style_3_8_icon {
	width: 34%;
}
.style_3_1_icon, .style_3_5_icon {
	width: 67%;
}
.style_3_content {
	display: block;
	margin: 6px;
	box-shadow: 0 0px 3px 0 rgba(0, 0, 0, 0.15);
	overflow: hidden;
}
.style_3_2_icon .style_3_content, .style_3_3_icon .style_3_content, .style_3_4_icon .style_3_content, .style_3_7_icon .style_3_content, .style_3_8_icon .style_3_content, .style_3_9_icon .style_3_content {
	text-align: center;
	color: #000;
	font-size: 16px;
	height: 48px;
	line-height: 48px;
	background: #fff;
}
.style_3_0_icon .style_3_content, .style_3_1_icon .style_3_content, .style_3_6_icon .style_3_content {
	height: 112px;
}
.style_3_noimg {
	height: 112px;
	background-image: url(../images/c.png);
	background-repeat: no-repeat;
	background-position: right bottom;
}
.style_3_noimg span {
	height: 44px;
	line-height: 44px;
	display: block;
	overflow: hidden;
	padding: 0 10px;
	font-size: 24px;
	color: #fff;
}
.style_3_img {
	height: 112px;
	overflow: hidden;
	position: relative;
}
.style_3_img img {
	width: 100%;
}
.style_3_img span {
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	height: 25px;
	line-height: 25px;
	display: block;
	overflow: hidden;
	text-align: center;
	color: #fff;
	background: rgba(0, 0, 0, 0.6);
}

.style_4_icon {
	height: 62px;
