body {
	background: #f0efef;
}
a {
	color: #986;
	text-decoration: none;
}
.top {
	background: #78bb6c url(../images/mtopbg.png) repeat-x bottom center;
	padding: 5px;
	border-bottom: 1px solid #6a5;
}
h1 {
	padding: 2px;
}
.top h1 {
	display: block;
	margin: 0;
	padding: 0;
	text-align: center;
	color: #fff;
	font-size: 20px;
	font-weight: bold;
}
.top a {
	color: #fff;
}
.top a:hover {
	text-decoration: none;
}

.list {
	margin: 15px;
	background: #fff;
	box-shadow: 0 1px 2px #ccc;
	border-radius: 5px;
}
.content {
	padding: 10px;
}
.info {
	font-size: 12px;
	padding: 10px;
	border-top: 1px solid #ccc;
	line-height: 20px;
}
.info span {
	color: #7b6;
	font-weight: bold;
}
.photo {
	width: 32px;
	height: 32px;
	background: #fff;
	padding: 3px;
	box-shadow: 1px 1px 2px #ccc;
	display: block;
	float: left;
	margin-right: 10px;
}

.page {
	text-align: center;
	padding: 20px;
}

#top_msg {
	margin: 20px;
	background: #68af02;
	color: #fff;
	border: 1px solid #5a851d;
	border-radius: 3px;
	padding: 10px;
	font-weight: bold;
}
.set_input {
	height: 22px;
	border: 1px solid #d3d3d3;
	border-radius: 3px;
	padding: 2px;
	margin: 1px;
}
.set_button {
	height: 31px;
	color: #fff;
	background: #4d910c url(../images/btbg.gif) repeat-x left top;
	border: 1px solid #3d810c;
	border-radius: 3px;
}
.form_line {
	padding: 5px;
}
.form_center {
	padding: 5px;
	text-align: center;
}
.vote_img {
	height: 10px;
	border-radius: 5px;
	background: #4d910c url(../images/btbg.gif) repeat-x left top;
	border: 1px solid #3d810c;
}
.da_line {
	color: #fff;
	background: #4d910c url(../images/btbg.gif) repeat-x left top;
	border: 1px solid #3d810c;
	border-radius: 3px;
	padding: 5px;
	margin: 10px;
}
#tj_bg {
	filter:alpha(opacity=50);
