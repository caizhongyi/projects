    <SCRIPT type="text/javascript">
        /**
         * 没个菜单的颜色不一样，四个一循环
         */
        function setColors(){
            var count = document.getElementById("menu").getElementsByTagName("li");
            for(var i=0;i<count.length;i++){
                if(i%4 == 0){
                    count[i].style.backgroundColor = "#d87570";
                }else if(i%4 == 1){
                    count[i].style.backgroundColor = "#bc4040";
                }else if(i%4 == 2){
                    count[i].style.backgroundColor = "#6f9e98";
                }else{
                    count[i].style.backgroundColor = "#b2cebf";
                }
            }
        }

