.menu-btn a {
	display: block;
	width: 100%;
	height: 100%;
	color: #FFF
}

#button-1.open {
	-webkit-transform: translate3d(90px, -13px, 0);
	-moz-transform: translate3d(90px, -13px, 0);
	-ms-transform: translate3d(90px, -13px, 0);
	-o-transform: translate3d(90px, -13px, 0);
	transform: translate3d(90px, -13px, 0);
}

