header {
    left: 0;
  /*  position: fixed;*/
    top: 41px;
    width: 100%;
    z-index: 99;
}
nav {
    background-color: #EB6619;
    width: 100%;
}
nav a {
    -moz-box-sizing: border-box;
    border-left: 1px solid #F29D6C;
    border-right: 1px solid #EB551A;
    display: inline-block;
    float: left;
    height: 45px;
    line-height: 45px;
    left: 0;
    margin: 0;
    text-align: center;
    top: 0;
    width: 25%;
}
nav ul ,nav li {margin: 0; padding:0; list-style: none;}
nav .more{ position: relative; display: block; z-index: 1;   }
nav .more ul{ display: none; position: absolute;right: 0; top: 45px; width: 25%; background: #EB6619; }
nav .more ul a{ float: none; display: block; width: auto;border-bottom: 1px solid #EB551A; border-top: 1px solid #F29D6C;}
nav .more.shown ul{ display: block;}

nav a.current {
    background-color: #FF9000;
}
nav a i {
    background-size: 120px 30px;
    display: inline-block;
    height: 30px;
    margin-top: 7px;
    overflow: hidden;
    width: 30px;
}
nav a:nth-child(1) i {
    background-position: -90px 0;
}
nav a:nth-child(2) i {
    background-position: 0 0;
}
nav a:nth-child(3) i {
    background-position: -30px 0;
}
nav a:nth-child(4) i {
    background-position: -60px 0;
}
.container {
    padding-top: 45px;
    width: auto;
}
.wrap {
    overflow: hidden;
}


.mobile-lists, .adsl-lists {
    padding: 10px 5px;
}
.mobile-lists .item, .adsl-lists .item {
    float: left;
    width: 50%;
}
.mobile-lists .item .inner, .adsl-lists .item .inner {
    background-color: #FFFFFF;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
    margin: 0 5px 10px;
}
.mobile-lists .item .thumb, .adsl-lists .item .thumb {
    overflow: hidden;
    text-align: center;
}
.mobile-lists .item .thumb a, .adsl-lists .item .thumb a {
    background-position: center center;
    background-repeat: no-repeat;
    background-size: contain;
    display: inline-block;
    height: 160px;
    position: relative;
    width: 100%;
}
.mobile-lists .item .info, .adsl-lists .item .info {
    border-top: 1px solid #DDDDDD;
    font-size: 1em;
    padding: 5px 4px;
    position: relative;
}
.mobile-lists .item .info .name, .adsl-lists .item .info .name {
    line-height: 26px;
    padding-bottom: 10px;
    width: 100%;
}
.mobile-lists .item .info .price, .adsl-lists .item .info .price {
    color: #FF3333;
    font-size: 14px;
}
.mobile-lists .item .info .btn, .adsl-lists .item .info .btn {
    bottom: 5px;
    position: absolute;
    right: 3px;
}
.mobile-lists .name a{ color: #333;}
.mobile-lists .item .info .price{ font-size: 12px;}
.header {
    background: none repeat scroll 0 0 rgba(235, 102, 25, 0.9);
    color: #FFFFFF;
    display: block;
    float: left;
    height: 45px;
    left: 0;
    position: fixed;
    text-align: left;
    top: 0;
    width: 100%;
    z-index: 1000;
}
.header a {
    color: #FFFFFF;
    display: inline-block;
    font-size: 20px;
    line-height: 45px;
    position: relative;
}
.header a.go_link {
    padding: 0 15px 0 25px;
}
.header a.go_link b {
    background: url("goLink_bg.png") no-repeat scroll left top / 50% 50% rgba(0, 0, 0, 0);
    display: inline-block;
    height: 24px;
    left: 9px;
    margin-top: -5px;
    position: absolute;
    top: 50%;
    width: 19px;
}
.header a.go_home {
    float: right;
    height: 45px;
    width: 45px;
}
.header a.go_home:after {
    background: url("goHome_bg.png") no-repeat scroll left top / 50% 50% rgba(0, 0, 0, 0);
    content: "";
    display: inline-block;
    height: 40px;
    left: 15px;
    position: absolute;
    top: 13px;
    width: 34px;
}

