.btn-buy,.btn-buy:hover{ background: #b6732c ;  color: #fff;  text-shadow: none; }
.btn-buy:active,.btn-buy:focus{ background: #955e26;color: #fff; }
.btn-addcard,.btn-addcard:hover{ background: #b21100 ;color: #fff;}
