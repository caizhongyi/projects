h1{ margin: 15px 0;  line-height: 1.2}

.form { color: #999; font-size: 12px;}
.form .price{ font-size: 16px; font-weight: bold; font-style: normal;}
.form strong{ color: #000; margin-right:5px;}
.form .form-group{ margin-bottom: 15px;}
.form .form-label{ color: #999; margin-right: 10px;}
.form .form-control{ margin-right: 10px;}

.box_swipe {
    overflow: hidden;
    position: relative;
    visibility: hidden;
}
.box_swipe ul {
    overflow: hidden;
    position: relative;
}
.box_swipe ul > li {
    float: left;
    position: relative;
    width: 100%;
}
.box_swipe ul li span {
    bottom: 5px;
    color: #FFFFFF;
    display: inline-block;
    left: 10px;
    position: absolute;
    width: 75%;
}
.box_swipe > ol {
    height: 20px;
    margin-top: -25px;
    padding-right: 15px;
    position: relative;
    text-align: center;
    z-index: 10;
}
.box_swipe > ol > li {
    background-color: #cdcdcd;
    border-radius: 8px;
    display: inline-block;
    height: 8px;
    margin: 5px 0;
    width: 8px;
}
.box_swipe > ol > li.on {
    background-color: rgba(235, 102, 25, 0.9);
}

.input-group,
.input-search {
    display: inline-block;
    vertical-align: middle;
}
.input-group [type=text],
.input-search [type=text],
.input-group [type=password],
.input-search [type=password],
.input-group .input,
.input-search .input {
    border-radius: 0;
}
.input-group [class*=btn-]:first-child,
.input-search [class*=btn-]:first-child {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}
.input-group input + [class*=btn-],
.input-search input + [class*=btn-],
.input-group [class*=btn-] + input,
.input-search [class*=btn-] + input {
    border-bottom-left-radius: 0;
    border-top-left-radius: 0;
}
.input-group .add-on,
.input-search .add-on {
    padding: 4px 10px;
    text-align: center;
    display: inline-block;
    vertical-align: middle;
    border: 1px solid #ddd;
}
.input-group .add-on + input,
.input-search .add-on + input,
.input-group input + .add-on,
.input-search input + .add-on,
.input-group .input-group-item,
.input-search .input-group-item {
    margin-left: -1px;
}
.input-group-space {
    display: inline-block;
    margin-left: -10px;
}
.input-group-space .btn,
.input-group-space input,
.input-group-space select,
.input-group-space .input-control {
    margin-left: 10px;
}

.input-spinner [type=text],.input-spinner .input{ width: 30px;}
.spinner-small [type=text],.spinner-small .input{ width: 20px;}
.spinner-mini [type=text],.spinner-mini .input{ width: 10px;}
.spinner-large [type=text],.spinner-large .input{ width: 50px;}
.spinner-xlarge [type=text],.spinner-xlarge .input{ width: 100px;}
.spinner-xxlarge [type=text],.spinner-xxlarge .input{ width: 200px;}
.spinner-xxxlarge [type=text],.spinner-xxxlarge .input{ width: 400px;}

.input-spinner .input-group-btn { display: inline-block; vertical-align: middle; margin-left: -1px;}
.input-spinner-vertical .add-on{ display: block; padding:0px 5px; line-height:14px; color: #333; }
.input-spinner-vertical .add-on +  .add-on{ margin-top: -2px;}

.input-spinner .add-on{ color: #333; padding: 5px 10px;}
