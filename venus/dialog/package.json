{
    "name": "alienjs-dialog",
    "title": "alienjs-dialog",
    "description": "弹出窗口, 支持ie7+",
    "version": "1.0.8",
    "main": "dist/index.js",
    "homepage": "http://www.alienjs.net",
    "author": "zhongyi cai <274142680@qq.com>",
    "repository": { },
    "keywords": [
        "alienjs-dialog",
        "alienjs",
        "alien",
        "pop",
        "jquery",
        "js",
        "dialog"
    ],
    "output" : [ "index.css" ],
    "dependencies": {
        "animate" : "^1.0.0",
        "jquery" : "^1.11.2",
        "jquery-transitionEnd" : "^1.0.0"
    },
    "devDependencies": {

    },
    "scripts": {
        "babel-cleancss" : "cleancss src/index.css --output dist/index.css --source-map",
        "babel-combiled" : "babel src/index.js --out-file dist/index.js  -m umd --source-maps inline --compact --no-comments",
        "babel-combiled-all" : "babel src --out-file dist/index.js  -M -m umd --source-maps inline --compact --no-comments",
        "babel-combiled-dir" : "babel src --out-dir dist -m umd --source-maps inline  --no-comments --compact true --no-comments",
        "babel-compress" : " npm run babel-cleancss && npm run babel-combiled ",
        "transpiler" : "compile-modules convert index.es6 -I src -o dist/index.js -f commonjs"
    }
}
