<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="SQL"
    targetNamespace="SQL.xsd"
    elementFormDefault="qualified"
    xmlns="SQL.xsd"
    xmlns:mstns="SQL.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:element name="SQLScript">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="SQL" minOccurs="1">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="mstns:NonEmptyStr">
                <xs:attribute name="ID" type="xs:ID" use="required"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="Module" maxOccurs="unbounded">
          <xs:complexType>
            <xs:choice maxOccurs="unbounded">
              <xs:element name="SQL" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="mstns:NonEmptyStr">
                      <xs:attribute name="ID" type="xs:string" use="required"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:attribute name="ID" type="mstns:NonEmptyStr" use="required"/>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="NonEmptyStr">
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
