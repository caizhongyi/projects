    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f46900), to(#ff9f00));
    background:-o-linear-gradient(top, #f46900, #ff9f00);
	color:#fff;	
}
.button3:hover{ text-decoration:none;box-shadow:0 0 4px #f46900; border-color:#f46900; }

.button4{
	border:1px solid #99C3DB;
	border-radius:5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	padding:2px 10px;
	filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f3fbfe,endcolorstr=#d2eaf6,gradientType=0);
