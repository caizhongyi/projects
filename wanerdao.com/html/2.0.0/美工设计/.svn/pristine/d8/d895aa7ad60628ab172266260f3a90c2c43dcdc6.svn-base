@charset "utf-8";
/* CSS Document */
/*=====Global=====*/
input,textarea,select{vertical-align:middle;}
.form{ font-size:14px; color:#bcc4c5;}
.form .label{ display:inline-block; margin-right:10px; vertical-align:middle; }
.form h3{ font-weight:bold; color:#333;}
.form h3 span{  margin:0 5px; font-size:12px; font-weight:normal;}
.form .wraplabel{ color:#333;font-size:12px;}
.form .wraplabel span{ color:#666666;}
.form .wraplabel span,.form .wraplabel a ,.form .wraplabel .text { margin-left:5px;}
/* select 修正 */
.form .wraplabel .chzn-single{ margin-left:0;}
.form .wraplabel .chzn-container{ margin-left:5px;}
/* select 修正 */
.form .text{ vertical-align:middle;}
.form li{ margin:15px 0; text-align:left;}
.form .submit{ text-align:center; margin:30px 0;}
.form .submit .button{ margin:0 10px;}
.form .result{ text-align:center; margin:30px 0;}


/*  textboxs  */

.text{ padding-left:5px;}
.text,.select,.chzn-container{ height:25px; line-height:25px; display:inline-block;  }
.text,.textarea,.labeltext,.select{border:1px solid #dfdfdf; border-radius:3px;-moz-border-radius: 3px;-webkit-border-radius: 3px;line-height:23px;background:#fff; box-shadow:inset 0 0 4px #dfdfdf; }
.textarea{padding:5px;}
/* text font */
.text,.select,.chzn-container,.chzn-container a,.labeltext{  color:#666; }

/* focus */
.text:focus,.select:focus,.textarea:focus,.labeltext-focus{ box-shadow:0 0 4px #89c6f5; border-color:#89c6f5; }

.select{ position:relative; top:-2px; padding-top:2px;}

.labeltext{display:inline-block; *display:inline; zoom:1; }
.labeltext,.labeltext input{padding:0 5px;height:30px; line-height:30px; }
.labeltext label{display:inline-block; padding:0 5px;}
.labeltext label,
.labeltext input{vertical-align:middle ;}
.labeltext input{border:none; background:none;} 
.labeltext img{ vertical-align:middle;}

.time-text{ background:url(../images/icons/icon-time.png) no-repeat 98% center; width:170px; padding-right:16px;}
/*  $textboxs  */


/* input右侧点击效果 */
.inputclick{ position:relative;}
.inputclick .clickbtn{ position:absolute; display:block; width:30px; height:25px; top:0; right:15px;} 

/*  buttons  */
.button{border:none; display:inline-block;text-align:center;cursor:pointer; overflow:hidden; }
.button:hover{ text-decoration:none; border-color:#89c6f5; /*border-radius:5px;*/ }

.button1{  
 /*background:url(../images/buttons/button_1.png) no-repeat;*/
	filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#36b7d9,endcolorstr=#39a4c1,gradientType=0);
    -ms-filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#36b7d9,endcolorstr=#39a4c1,gradientType=0);/*IE8*/  
    background:#39a4c1; /* 一些不支持背景渐变的浏览器 */
	/*background:-ms-linear-gradient(top, #36b7d9, #39a4c1 50%, turquoise);*/
    background:-moz-linear-gradient(top, #36b7d9, #39a4c1);
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#36b7d9), to(#39a4c1));
    background:-o-linear-gradient(top, #36b7d9, #39a4c1);
	border:1px solid #99c3db;
	box-shadow:0 0 4px #89c6f5;
	font-weight:bold;
}
.button1:active{
	*filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#39a4c1,endcolorstr=#36b7d9,gradientType=0);
    -ms-filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#39a4c1,endcolorstr=#36b7d9,gradientType=0);/*IE8*/  
    background:#36b7d9; /* 一些不支持背景渐变的浏览器 */
    background:-moz-linear-gradient(top, #39a4c1, #36b7d9);
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#39a4c1), to(#36b7d9));
    background:-o-linear-gradient(top, #39a4c1 ,#36b7d9);
}
.button1-disable{
	filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#e9e9e9,endcolorstr=#dddddd,gradientType=0);
    -ms-filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#e9e9e9,endcolorstr=#dddddd,gradientType=0);/*IE8*/  
    background:#dddddd; /* 一些不支持背景渐变的浏览器 */
    background:-moz-linear-gradient(top, #e9e9e9, #dddddd);
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#e9e9e9), to(#dddddd));
    background:-o-linear-gradient(top, #e9e9e9, #dddddd);
	border:1px solid #dddddd;
}

.button1,.button1:active,.button1-disable{
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius:5px;
	padding:5px 30px;
	color:#fff; 
	text-shadow:1px 1px 2px #000; 
	box-shadow:0px 1px  2px #666;
}

.button2{
    border:1px solid #99c3db;
    filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f3fbfe,endcolorstr=#d2eaf6,gradientType=0);
    -ms-filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f3fbfe,endcolorstr=#d2eaf6,gradientType=0);/*IE8*/  
    background:#e7f4fc; /* 一些不支持背景渐变的浏览器 */
    background:-moz-linear-gradient(top, #f3fbfe, #d2eaf6);
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f3fbfe), to(#d2eaf6));
    background:-o-linear-gradient(top, #f3fbfe, #d2eaf6);
	padding:4px 10px;
	color:#1992B1;
}

.button3{
	border:1px solid #dc6900;
	border-radius:5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	padding:2px 10px;
	filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f46900,endcolorstr=#ff9f00,gradientType=0);
