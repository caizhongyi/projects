/* 蓝色和灰色圆角按扭 */
.blue-radius-btn,
.gay-radius-btn,
.disable-radius-btn{text-align: center; padding: 0 30px; height: 35px; line-height: 35px;font-size: 14px; border-radius: 3px; background: url(../../images/buttons/blue-gay-btn.gif) repeat-x;}
a.blue-radius-btn,
a.gay-radius-btn,
a.disable-radius-btn{ height: 33px; line-height: 33px;}
.blue-radius-btn{ border:1px solid #dadada ;  font-weight: bold; color: #fff;  }
.disable-radius-btn,
.gay-radius-btn{ border:1px solid #dadada ;  font-weight: bold; color: #fff;background-position: 0 -68px; }
.disable-radius-btn{ cursor:default;}
.disable-radius-btn:hover{border:1px solid #dadada ;}
.blue-radius-btn:active{ background-position: 0 -34px;}
.gay-radius-btn:active{ background-position: 0 -102px;}

