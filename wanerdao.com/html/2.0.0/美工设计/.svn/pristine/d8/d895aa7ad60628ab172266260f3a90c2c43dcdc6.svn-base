}

.button-gay{
	border:1px solid #c4c5c4;
	padding:2px 10px;
	filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f7f4f4,endcolorstr=#e0e1e0,gradientType=0);
    -ms-filter:alpha(opacity=100,finishopacity=50,style=1,startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f7f4f4,endcolorstr=#e0e1e0,gradientType=0);/*IE8*/
    background:#e0e1e0; /* 一些不支持背景渐变的浏览器 */
    background:-moz-linear-gradient(top, #f7f4f4, #e0e1e0);
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f7f4f4), to(#e0e1e0));
    background:-o-linear-gradient(top, #f7f4f4, #e0e1e0);
	color:#666;	
	
}
.button-gay:hover{ border-color:#c4c5c4;border-radius:0;}

.blue-button{
	background: none repeat scroll 0 0 #91CEDD;
    border: 1px solid #60B7CD;
    border-radius: 4px 4px 4px 4px;
    color: #FFFFFF;
    line-height: normal;
    padding: 2px 15px;
    text-shadow: 1px 1px 1px #5DB1C6;
}
.blue-button .icon{ vertical-align:top; margin:2px 4px 2px 2px;}
.blue-button:hover{ border-color:#36B7D9;}

.radius-gay-button{
	background: none repeat scroll 0 0 #f8f8f8;
    border: 1px solid #d8d9db;
    border-radius: 4px 4px 4px 4px;
	line-height: normal;
    padding: 1px;
    padding-right: 10px;
	text-shadow: 0px 0px 1px #999;
	color:#999;
}
.radius-gay-button .icon{ vertical-align:middle; margin-right:10px; display:inline-block;}

.radius-blue-button{
    background: none repeat scroll 0 0 #91cedd;
    border: 1px solid #5fb7cd;
    border-radius: 4px 4px 4px 4px;
    line-height: normal;
    padding: 1px;
    padding-right: 10px;
    text-shadow: 0px 0px 1px #5eb0c5;
    color:#fff;
}
.radius-blue-button .icon{ vertical-align:middle; margin-right:10px; display:inline-block;}


.join-button{ background:url(../images/buttons/join_151x28.png) no-repeat;width:121px; height:28px; line-height:28px;  color:#0063a7; padding-left:30px;}
