<script type="text/javascript">
function showmenu(id,obj2){
	obj = $("#"+id);
	left2 = $(obj2).offset().left;
	top2 = $(obj2).offset().top+$(obj2).height()-1;
	
	obj.css('position','absolute');
	obj.css('top',top2);
	obj.css('left',left2);
	obj.show();

	obj.find('li').each(
		function(){
			$(this).click(function(){
				html = $(this).html();
				$(obj2).val(html);
			});
		}
	);
	
	$(obj2).mouseout(function(){if($(this).attr('show')==undefined){obj.hide()}});

	obj.mouseout(
		function(){
			$(obj2).removeAttr('show');
			$(this).hide();
		}
	);
	obj.mouseover(
		function(){
			$(obj2).attr('show',1);
			$(this).show();
		}
	);
}
</script>
