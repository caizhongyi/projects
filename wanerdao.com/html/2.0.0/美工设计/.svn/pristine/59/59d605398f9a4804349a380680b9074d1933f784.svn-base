.button{ cursor:pointer;}
.text{ border:1px solid #ddd; background:#fff;}
.bold{ font-weight:bold;}
/*  clear  */
.clear{display:block;height:0;line-height:0;font-size:0;visibility:hidden;clear:both;}
.clearfix{zoom:1;}
.clearfix:after{content:".";height:1%;display:block;height:0;line-height:0;font-size:0;visibility:hidden;clear:both; *display:none;} 
/*  end clear  */

/* template */
.side{ float:left; _margin-right:-3px;}
.subside{ float:right; _margin-left:-3px;}
.main{ overflow:hidden;} 
/* end template */

/* mod */
.hd{padding-left:10px;}
.bd{padding:10px;}
.ft{text-align:center;}
/* end  mod */

.left { float:left;*display:inline;_margin-right:-3px; }
.right { float:right;*display:inline;_margin-left:-3px; } 
.inline 
{ 
	 display:-moz-inline-stack;
	 display: inline-block;     
	 zoom: 1; *display: inline; /* IE < 8: fake inline-block */     
	 letter-spacing: normal;     
	 word-spacing: normal;      
}

.disable {color:#999; text-decoration:none; cursor:default;}
.disable:hover{ text-decoration:none;} 

.info-container{ padding:15px 30px;}

.subChaTab .mychanel,.subChaTab .mychanel:hover{ background:url(../images/information/mychanel.gif) no-repeat; padding-left:20px; float:right; color:#636466; width:73px; height:28px; font-weight:normal; font-size:12px; margin-top:5px; ; line-height:normal; padding-top:8px;}

/* 字母排序 */
.letter-order{}
.letter-order .letters{ background:#eef7fe; padding:5px 10px; margin:10px 0;  border-bottom:1px solid #9ecfdc;}
.letter-order .letters a{ color:#000000; margin:0 5px;}
.letter-order .letters .all{ color:#fff; padding:5px 7px; background:#1682a6;}
.letter-order ul{ margin:10px 0;}
.letter-order li{ margin:25px 0;}
.letter-order li .hd{ font-weight:bold; background:url(../images/information/letter-hd.gif) no-repeat; color:#fff; padding-left:17px; height:19px; line-height:20px; border-bottom:1px solid #9ecfdc;}
.letter-order li .even{ background:url(../images/information/letter-hd-even.gif) no-repeat;}
.letter-order .letter-items li{ float:left; width:131px; margin:5px 0; font-size:14px; color:#cccccc;}
.letter-order .letter-items li a{ color:#337fa5; margin:0 3px;}
