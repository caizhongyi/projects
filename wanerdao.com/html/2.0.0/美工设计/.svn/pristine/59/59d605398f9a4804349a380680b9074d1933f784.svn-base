.class-order li{ float:left; width:230px; _margin-right:-3px;}
.class-order dl{ margin-bottom:60px;}
.class-order dt{ font-weight:bold; color:#000; background:url(../images/information/icos-info.png) no-repeat 0 2px; padding-left:25px; margin:15px 0;}
.class-order dt.ico-1{ background-position:0 -20px;}
.class-order dt.ico-2{ background-position:0 -45px;}
.class-order dt.ico-3{ background-position:0 -74px;}
.class-order dt.ico-4{ background-position:0 -45px;}
.class-order dt.ico-5{ background-position:0 -20px;}
.class-order dt.ico-6{ background-position:0 -148px;}
.class-order dt.ico-7{ background-position:0 -177px;}
.class-order dt.ico-8{ background-position:0 -202px;}
.class-order dt.ico-9{ background-position:0 -225px;}

.class-order dd{ margin:10px 0; padding-left:10px; color:#999999; }
.class-order dd a{ color:#337fa5; margin-right:5px;}
.class-order dd span{ font-family:Tahoma, Geneva, sans-serif; font-size:12px; color:#ccc;}
