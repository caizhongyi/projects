<script type="text/javascript">
   (function(){
	   
	   /* replay */
	   $('.info-replay').stop().animate({'height' : 'hide'});
	   var infoList  = $('#info-list').children();
	   infoList.hover(function(){
	   		$(this).find('.userlike').stop(true,true).show();
	   },function(){
			$(this).find('.userlike').stop(true,true).hide();
		})
	   infoList.find('.show').click(function(){
		   $(this).parentsUntil('li').find('.info-replay').stop().animate({'height' : 'show'});
		   $(this).hide();
		   $(this).parent().find('.hide').show();
	   })
		infoList.find('.hide').click(function(){
			$(this).parentsUntil('li').find('.info-replay').stop().animate({'height' : 'hide'});
			$(this).hide();
			$(this).parent().find('.show').show();
	   })
	   infoList.find('.opts-return').click(function(){
		   var parent = $(this).parentsUntil('li');
	       parent.find('.info-replay').stop().animate({'height' : 'hide'});
		   parent.find('.hide').hide();
		   parent.find('.show').show();
	   });
	  
	   /* replay */
	   
	   /* 输入框伸缩 */
	   var infoForm = $('.info-form');
	   infoForm.find('.bd').animate({'height':'hide'});
	   infoForm.find('#share').click(function(){
		   infoForm.find('.bd').animate({'height':'toggle'});
		});
	   infoForm.find('.cancel').click(function(){
		   infoForm.find('.bd').animate({'height':'hide'});
		});
	   /* 输入框伸缩 */
   })()
</script>

