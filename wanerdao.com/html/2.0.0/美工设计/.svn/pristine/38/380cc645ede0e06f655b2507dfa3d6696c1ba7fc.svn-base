			myhistorycontents = $('#myhistorycontent').children();
		myhistoryMenuA.click(function(){
			myhistoryMenuA.removeClass('current');
			$(this).addClass('current');
			myhistorycontents.hide();
			
			myhistorycontents.eq($(this).parent().index()).stop(true,true).fadeIn();
		})
	})
 	
