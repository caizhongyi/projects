<script type="text/javascript">
	$('.activityList_Will li').click(function(){
		var item = $('.activityDetailList', $(this));
		if(item.is(':hidden')){
	    	$(this).css('background','#f7f7f7');
			$('.activityDetailList', $(this)).show();
		}
		else{
			$(this).css('background','');
			$('.activityDetailList', $(this)).hide();
		}
	})
