    $('.activityList_Will').off('click.activityList').on('click.activityList','li',function(){
        if($(this).find('.activityDetailList').is(':animated')){ return ;}
        var items = $('.activityList_Will > li');
        items.not(this).removeClass('switchover').find('.activityDetailList').stop(true).slideUp();
        items.find('.back').hide();
        items.eq($(this).index()).addClass('switchover').find('.activityDetailList').stop(true).slideDown();
        $(this).find('.back').show();
    }).off('click.back').on('click.back','.back',function(e){
        e.stopPropagation();
        $(this).hide().prev().stop(true).slideUp();
        $(this).closest('li').removeClass('switchover');
    });




