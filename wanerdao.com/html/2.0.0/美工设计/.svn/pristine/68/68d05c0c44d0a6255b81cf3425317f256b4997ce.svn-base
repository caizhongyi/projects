				$(this).css("backgroundColor","#fff");
				$(this).find(".listbox_ri").hide();
			}	
		);

		$(this).mouseover(
			function(){
				$(this).css("backgroundColor","#f7f7f7");
				$(this).find(".listbox_ri").show();
			}	
