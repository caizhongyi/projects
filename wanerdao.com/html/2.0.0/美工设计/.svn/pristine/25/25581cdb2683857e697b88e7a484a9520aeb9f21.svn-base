@charset "utf-8";
/* CSS Document */

.message_invite img {
	padding:1px;
	border:1px solid #b3dfee;
	margin-left:10px;
}
.message_invite tr {
	height:80px;
}
.downLoadMore a {
	font:200 12px/35px Arial, Tahoma;
	color:#7d7d7d;
}
