			</div>';
            var $dialog = $(html).css({width:size.width, height:size.height}).appendTo($('body'))
            this.dialog = new $.ui.dialog($dialog, {
                callback:{ hide:function () {
                    $dialog.remove()
                }},
                widget:{
                    hide:'.close-3'
                }
            }).show();
            return this;
        },
        hide:function () {

            this.dialog && this.dialog.hide();
            return this;
        },
        conform:function (msg, iconClass) {
            var html = '<div style="width:500px; margin:10px auto;" class="pop">\
						<div class="pop-bg"></div>\
						<div class="pop-container">\
							<div class="pop-bd pop-dialog clearfix" ><span class="icon ' + iconClass + '" style="margin-left:50px;"></span><span class="overflow" style="width:300px; display:inline-block;vertical-align:middle;" >' + msg + '</span></div>\
							<div class="pop-ft">\
								<a class="button button1" href="javascript:;" onclick="pop.hide()">确定</a>\
								<a class="button button1-disable" href="javascript:;" onclick="pop.hide()">取消</a>\
							</div>\
						</div>\
					</div>';
            var $dialog = $(html).appendTo($('body'))
            this.dialog = new $.ui.dialog($dialog, {
                callback:{ hide:function () {
                    $dialog.remove()
                }},
                widget:{
                    hide:'.close-3'
                }
            }).show();
            return this;
        },
        warnning:function (msg) {
            this.conform(msg, 'warnning');
            return this;
        },
        error:function (msg) {
            this.conform(msg, '');
            return this;
        },
        message:function (msg) {
            this.conform(msg, 'message');
        }
    }


    pop.show('msg','title',{ width : 500})
    //pop.warnning('msg')
    //pop.message('确定确定确定确定确定确定确定确定确定确定确定确定确定确定确定确定确定确定确定')

