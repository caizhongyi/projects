@charset "utf-8";
/* CSS Document */
/*=====activity_create=====*/
/*----layout----*/
.activity_create{margin:0 auto; overflow:hidden;}
/*main*/
.activity_createMainList{ width:779px; height:auto; float:left; padding:10px 15px;}
.activity_createMainList h2{width:769px; padding:0 0 0 10px; font-size:16px; font-family:"微软雅黑";}
/*title*/
.activity_createMainList .mainTitle{width:779px; height:32px; margin:0 auto;}
/*modSwitch*/
.activity_createMainList .modSwitch{width:769px;height:32px; line-height:31px; margin:5px auto;  padding:0 0 0 10px;}
/*widget*/
.activity_createWidget{width:195px; height:auto; float:left;}
/*----form----*/
label{ margin:0 3px 0 3px;color:#666;}
.radio{ margin:0 3px 0 0;}
.select{ margin:0 8px 0 8px;}
/*guideMod  向导模式*/
.activity_createMainGuide{ width:970px; margin:0 auto;  padding:10px 15px;}
.activity_createMainGuide h2{width:960px; padding:0 0 0 10px; font-size:16px; font-family:"微软雅黑";}
.activity_createMainGuide .mainTitle{width:970px; height:32px; margin:0 auto;}
.activity_createMainGuide .modSwitch{width:970px;height:32px; line-height:31px; margin:5px auto;  padding:0 0 0 10px;}
.ACmodGuide{width:960px; border-bottom:1px dotted #add3c8; margin:0 auto; padding:10px 0;}
.ACmodGuide h3{ width:960px; height:32px; line-height:31px; background:#eef9fc; color:#666666; padding:0 0 0 10px;}
.ACmodGuide h4{width:760px; height:32px; line-height:31px; font-size:12px; color:#666; border-bottom:1px dotted #add3c8; padding:0 0 0 20px; margin:0 auto;}
.ACmodGuide h4 .checkbox{ margin: 0 4px 0 0;}
.ACmodGuide h5{margin:10px 0 0 0;}
.ACmodGuideTitle{width:auto; height:16px; padding:0 0 0 18px;}
.ACmodGuide .applySet{display:block; width:780px;}
.ACmodGuide .applyFeeSet{display:block;width:780px;}
.ACmodGuide .thresholdSet{display:block;width:780px;}
.ACmodGuide .preparedbudget{display:block;width:780px;}
.activity_createMainGuide .btn-warp{ width:950px;  height:34px; line-height:33px; margin:10px auto; text-align:left; padding:0; overflow:hidden;}
/*listMod 列表模式*/
.ACmodList{width:769px; padding:0 0 0 10px; margin:10px auto;}
.ACmodList h3{ width:759px; height:32px; line-height:31px; background:#eef9fc; color:#666666; padding:0 0 0 10px;}
.ACmodList h4{width:729px; height:32px; line-height:31px; font-size:12px; color:#666; border-bottom:1px dotted #add3c8; padding:0 0 0 20px; margin:0 auto;}
.ACmodList h4 .checkbox{ margin: 0 4px 0 0;}
.ACmodList h5{margin:10px 0 0 0;}
.ACmodListTitle{width:auto; height:16px; padding:0 0 0 18px;}
.ACmodList .applySet{display:none;}
.ACmodList .applyFeeSet{display:none;}
.ACmodList .thresholdSet{display:none;}
.ACmodList .preparedbudget{display:none;}
/*setmod*/
.setmodList{width:auto; height:auto;padding:0 10px 0 10px; margin:0 auto; color:#949494; }
.setmodList dl{width:auto; height:auto; margin:8px auto;}
.setmodList dd.formTitle{float:left; text-align:right; line-height:33px;}
.setmodList dd.formMain{float:left; text-align:left; line-height:33px;}
.setmodList dd.formMain .textarea{ margin-top:3px;}
/*====basicQuote====*/
.basicQuote dd.formTitle{width:180px;}
.basicQuoteTip{ left:320px; top:-5px;}
.basicQuoteTip{ *+line-height:24px;}
/*====basicSet====*/
.basicSet dd.formTitle{width:160px;}
/*====addressSet====*/
.addressSet {padding:15px 0 15px 0; height:auto; overflow:hidden;}
.addressSet dd.formTitle{width:160px;}
.setBar,.address-show{width:693px;height:33px; border:1px solid #ddd; background:#ebebeb; margin:0 auto;}
.setBar li{ width:58px;height:33px; line-height:32px; padding:0 5px; margin:0 0 0 12px;float:left; }
.setBar #switchBtn{display:block; margin:5px 0 0 0;}
.currentSch{font-weight:bold; color:#1383a9;}
.address-show{ line-height:33px;width:693px; padding:0; }
.address-show .MapSchBar{ padding:0; float:none; width:auto; background:#F4F4F4; text-align:center;}

.listSchContent .descript{overflow: hidden;white-space: nowrap;-o-text-overflow: ellipsis;_text-overflow: ellipsis; text-overflow: ellipsis; width:600px;}
.listSchContent .descript:hover{overflow:visible;white-space:normal;}
/*---ListSch---*/
.listSchBar{width:693px;height:64px;border:1px solid #dcdcdc; border-bottom:none;border-top:none; background:#f4f4f4; margin:0 auto;}
.activityCat {width:643px; margin:0 auto; height:64px; padding:10px 0 0 0;}
.activityCat li{float:left; padding:0 4px; height:22px; text-align:center; margin:10px 0 10px 0;}
.AC_catList{ width:544px; overflow:hidden;}
.AC_catList li{ width: auto; height: auto;font-size:12px; padding:0 5px; float:left; text-align:center; margin:0;}
.AC_catList li a{ width: auto; height: auto; padding:0 5px; font-size:12px; color:#1383a9; text-align:center; display:block; border-radius: 5px; background: #eef7fe; border: 1px solid #1383A9;}
.AC_catList li .cat_btn-selected,.AC_catList li .cat_btn:active{
    width: auto; height: auto; padding:0 5px;background: #ddd; border: 1px solid #999;
}
.AC_catList li a:hover{ text-decoration:none;}
.listSchContent{ width:693px;border:1px solid #ddd; margin:0 auto;}
.listSchContent .pageList{ width:673px; text-align:right; background:#fbfbfb; height:24px; line-height:23px; margin:10px auto;}
.listSchContent li:hover{ background:#EEF7FE;  border-bottom:1px dashed #5fb0d3; position:relative;}
.listSchContent li:hover .button{display: block;}
.listSchContent li{ line-height:25px; border-bottom:1px dashed #999; margin:0 10px; padding:10px 5px;position: relative;}
.listSchContent .button{ position:absolute; right:10px; top:10px; display: none;}
/*---MapSch---*/
.mapschWarp{width:693px;margin:0 auto; display:none; border:1px solid #dcdcdc;}
.MapSchL,.MapSchBar{float:left;background:#eef7fe;}
.MapSchR{float:right;background:#eef7fe;}
.MapSchBar{width:680px;height:34px; line-height:33px; padding:0 0 0 12px; font-size:12px; color:#666; border-top:1px solid #1685a8;}
.MapSchR{width:215px; height:462px; border:1px solid #c8e7ec;}
.MapSchR li{width:192px; line-height:24px;  margin:5px auto;}
.MapSchR li .upArrow_G{left:90px}
.MapSchR .checkbox{ margin: 0 4px 0 0;}
.MapNote{height:28px; line-height:29px;background:#f7f7f7; font-size:12px;}
.MapNote p{height:28px; line-height:29px; padding:0 0 0 18px;}
.MapNote p img{ margin:0 8px 0 8px;}
/*====activityParameter====*/
.activityParameter dd.formTitle{width:160px;}
.activityParameter .tips_G{ margin:0 0 0 160px; *+margin:0;}
.activityParameter .upArrow_G{left:240px;top:33px;*+top:0;}
/*====preparedPlan====*/
.preparedPlan dd.formTitle{width:160px;}
/*---timeline---*/
.timelineWarp{ width:718px; margin:10px auto; position:relative;}
.timelineTop{height:30px; position:relative; display:block; }
.tips_B{ position:absolute; display:none;width:79px; height:28px; background:url(../images/timeline/bg_richeng_showdate.png) no-repeat; line-height:19px; text-align:center; left:-20px; top:-30px;}
.timelineBtm{height:80px; position:relative; margin:8px 0 0 0;}

.timelineMain{ margin:10px 0; width:700px; height:24px; line-height:23px; background:url(../images/timeline/bg_richeng.png) left  center no-repeat; padding:0 5px;}
.timelineMain li{float:left; background:url(../images/timeline/bg_richeng_round.png) left center no-repeat; width:20px; height:21px; margin:2px 4px 0 4.8px;}
.timelineMain li:hover .tipsWarp,
.timelineMain li:hover .tips_B{ display:block; z-index:1;}
.timelineMain li.current{ width:23px; height:24px; background:url(../images/timeline/bg_richeng_cur.png) left center no-repeat;}
/*applySet*/
.applySet dd.formTitle{width:160px;}
.applySet .upArrow_G{left:208px; *+left:227px;}
.applySet .pwdWarp{ width:227px; float:left; text-align:center;}
/*applyFeeSet*/
.applyFeeSet dd.formTitle{width:160px;}
/*thresholdSet*/
.thresholdSet dd.formTitle{width:160px;}
/*preparedbudget*/
.preparedbudget dd.formTitle{width:160px;}
/*sendInvite*/
.sendInvite dd.formTitle{width:160px;}
.inviteTableWarp{width:484px; margin:0 auto;}
.inviteTableWarp dd.formTitle{width:160px; padding:0;}
.inviteTable{border:1px solid #dfdfdf; margin:0 auto; width:484px; height:327px;}
.sendInvite .upArrow_G{ left:190px;}
/*applyPerson*/
.applyPerson dd.formTitle{width:160px;}
/*saveParameter*/
.saveParameter{border-bottom:1px dotted #add3c8;}
.saveParameterWarp{width:500px; background:#f7f7f7; padding:0 30px; overflow:hidden; border-radius:7px; border:1px solid #eee; margin:15px auto;}
.saveParameterForm{ margin:0 0 0 20px;}
.saveParameterWarp .upArrow_G{left:240px;}
/*btnWarp*/
.activity_createMainList .btn-warp{ width:729px;  height:34px; line-height:33px; margin:0 auto; text-align:left; padding:0;}
/* widget */
.verifyWarp{ width:170px; height:auto; background:#1992b1;overflow:hidden;/*CSS3*/ box-shadow:1px 2px 2px #666; border-top-right-radius:5px; border-bottom-right-radius:5px; padding:5px 0; top:0;left:0;}
.verifyWarp li{ height:24px; line-height:23px; margin:0 auto; padding:0 0 0 40px;}
.verifyWarp li span{margin:4px 24px 0 0; width:16px; height:16px; display:block;}
.verifyWarp li a{ color:#fff;}
.verifyWarp li a:hover{text-decoration:none;}

