@charset "utf-8";
/* CSS Document */
/*=====activity_create=====*/
.activity_create{margin:0 auto; overflow:hidden;}
.activity_create h3{font-size:16px; height:34px; line-height:33px;}
.activity_create textarea{ vertical-align:text-top; line-height:23px;}
/*----list mod----*/
.AC_List{ width:779px; height:auto;overflow:hidden; padding:0 15px;}
.AC_List h2{border-bottom:1px solid #9fcfd9; margin:22px auto;}
.AC_List .ACmod{width:779px; margin:0 auto;}
.ACmod h4{font-size:14px; height:34px; line-height:33px; padding:0 0 0 20px;}
.ACmod .downarrow_B,
.ACmod .uparrow_B{ width:16px; height:16px; background:url(../images/icons/icon16.png) no-repeat; display:block; margin:7px 12px 0 0; cursor:pointer;}
.ACmod .uparrow_B{ background-position: -128px 0;}
.ACmod .downarrow_B{ background-position: -128px -16px;}
.ACmod h5{ width:729px; height:34px; line-height:33px; border-bottom:1px dotted #add3c8; padding:0 0 0 20px; margin:0 auto;}
.AC_List .setMod{width:779px; padding:15px 0 0 0; margin:10px auto; display:none;}
.AC_List .setMod dl{width:650px;line-height:33px; padding:5px 0 0 0;}
.formTitle{width:150px; float:left; text-align:right; color:#7e7e7e;}
.formMain{float:left; color:#7e7e7e;} 
.setMod select{margin:0 4px 0 4px;}
.AC_List .sendInviteLayout{width:484px;margin:0 auto; padding:0;line-height:23px; display:none;}
.AC_List .sendInviteLayout dl{width:484px; line-height:33px; padding:5px 0 0 0;}
.AC_List .saveParameter{border-bottom:1px dotted #add3c8;}

/*----AC Widget----*/
.AC_Widget{ width:195px; height:100%;}
.verifyWarp{ width:170px; height:auto; background:#1992b1;overflow:hidden;/*CSS3*/ box-shadow:1px 2px 2px #666; border-top-right-radius:5px; border-bottom-right-radius:5px; padding:5px 0; top:0;left:0;}
.verifyWarp li{ width:138px; height:24px; line-height:23px; margin:0 auto; padding:0 0 0 40px;}
.verifyWarp li span{margin:4px 24px 0 0; width:16px; height:16px; display:block;}
.verifyWarp li a{ color:#fff;}
.verifyWarp li a:hover{text-decoration:none;}
.AC_List .btn-warp{ height:80px; line-height:79px; margin:5px 0 0 0;}
/*----Guide mod----*/
.AC_Guide{width:970px; height:auto; overflow:hidden; margin:0 auto;}
.AC_Guide h2{ border-bottom:1px solid #9fcfd9; margin:22px auto;}
.AC_Guide .ACmod{width:870px; margin:0 auto; border-bottom:1px dotted #add3c8; padding:0 0 20px 0;}
.AC_Guide h3{font-size:16px; border-bottom:1px dotted #add3c8;}
.AC_Guide h4{font-size:14px; height:34px; line-height:33px; padding:0 0 0 20px; margin:10px auto;}
.AC_Guide .setMod{width:779px; padding:15px 0 0 0; margin:10px auto;display:none;}
.AC_Guide .setMod h5{ height:34px; line-height:33px; border-bottom:1px dotted #add3c8; padding:0 0 0 20px;}

.AC_Guide .setMod dl{width:650px; margin:10px auto;  line-height:33px;padding:5px 0 0 0;}
.AC_Guide .setMod input {margin:0 4px 0 0;}
.AC_Guide .setMod select{margin:0 4px 0 4px;}
.AC_Guide .sendInviteLayout{width:484px;margin:0 auto; padding:0;line-height:23px;}
.AC_Guide .sendInviteLayout dl{width:484px; line-height:33px; padding:5px 0 0 0;}
.AC_Guide .saveParameter{border-bottom:1px dotted #add3c8;}
.AC_Guide .btn-warp{ width:800px;height:80px; line-height:79px; margin:5px auto; text-align:right;}


/*=====My History=======*/
.activity_myhistory{margin:0 auto; overflow:hidden;}
/* myhistroyTitle */
.myhistroyTitle{width:970px; margin:0 auto; height:auto;/*border-bottom:1px solid #9fcfd9;*/ margin:10px auto 0 auto; overflow:hidden;}
.myhistroyTitle li{float:left; margin:0 2px 0 0;}
.myhistroyTitle li a{ display:block;}
.myhistoryMenu li.setMod{ margin:20px auto;}
.MyactivitySet_Main dl{ padding:5px 0 5px 0;}
/* myhistoryMenu */
.myhistoryMenu{width:970px; margin:0 auto;line-height:47px; border-bottom:1px solid #1283a3; color:#7e7e7e; overflow:hidden;}
.myhistoryMenu li{line-height:29px;float:left; padding:0 4px; height:29px; }
.myhistoryMenu li a{ display:block;}
.myhistoryMenu li a:hover,
.myhistoryMenu li a.current{border-bottom:4px solid #1283a3; font-weight:bold;}

/*photoEdit*/
.AM_photoEditWarp{width:970px; margin:0 auto;}
.AM_photoEditWarp h4{ height:30px; line-height:29px; color:#7e7e7e;}
.AM_photoEdit_Main{ width:705px; height:auto;overflow:hidden; padding:0 0 0 15px; margin:20px 0 0 0; }
.AM_photoEdit_Main .pageList a:hover{text-decoration:none;color:#ea7726;}
.AM_photoEdit_Widget{width:248px; height:auto; background:#f8faf7; border:1px solid #e3e3e3;margin:20px 0 0 0;}
.photoAlbum{margin:0 auto;}
.upBtn{ width:220px;height:30px; background:#ccc;  margin:8px auto;}
.downBtn{width:220px;height:30px;background:#ccc; margin:8px auto;}
.photoAlbumList{width:222px;height:auto; margin:0 auto;}
.photoAlbumList li{ width:210px; height:170px; background:#deedff; border:1px solid #eff6fe; text-align:center; margin:8px 0 0 0; /*css3*/ box-shadow:0 1px 3px #ccc;border-radius:3px;-moz-border-radius: 3px;-webkit-border-radius: 3px; padding:5px 5px;}
.photoAlbumList li p{text-align:left; color:#000; height:34px; line-height:33px; padding:0 2px;}
.photoAlbumName{width:667px; height:48px; line-height:47px; padding:0 0 0 36px; background:#f9fcff url(../images/icons/AlbumName.png) left center no-repeat;border:1px solid #c9e0ee; margin:0 auto;}
.AlbumList { width:auto; overflow:hidden; margin:10px auto; border-bottom:1px dotted #7e7e7e;}
.AlbumList li{ width:210px; height:auto;border:1px solid #eff6fe; text-align:center; margin:8px 0 0 0; /*css3*/ box-shadow:0 1px 3px #ccc;border-radius:3px;-moz-border-radius: 3px;-webkit-border-radius: 3px; padding:5px 5px; position:relative; overflow:hidden; float:left; margin:10px 10px 0 10px;}
.AlbumList li p{text-align:left; color:#000; height:34px; line-height:33px; padding:0 2px;}
.AlbumListFoot{ padding:0 15px; height:32px; line-height:31px; overflow:hidden;margin:5px auto;}
.albumTitle{text-align:left; padding: 0 0 0 5px; height:24px; line-height:24px;}
.albumOpation{text-align:left; padding: 0 0 0 5px; height:24px; line-height:24px; display:none;}
.search-textbox input[type=text]{width:178px; margin-left:18px; color:#666; padding:0 5px;}
/*photo list*/
.photoList {width:705px; height:auto; }
.photoListHead {width:705px; height:32px; line-height:31px; border-bottom:1px dotted #7f7f7f;}
.photoListFoot {width:705px; height:60px; line-height:31px; color:#7f7f7f;}
.photoListMain li{ border-bottom:1px dotted #7f7f7f; padding:10px 0; color:#7f7f7f; margin:15px 0 0 0;}
.photoListMain li tr{line-height:37px;}
.operatBox{display:none;}
.operatBox a{ margin-right:5px; text-decoration:none;}
.operatBox a:hover{ margin-right:5px; text-decoration:none;}
.photoListMain li:hover{background:#f9f9f9;}
.photoListMain li:hover .operatBox{display:block;}
