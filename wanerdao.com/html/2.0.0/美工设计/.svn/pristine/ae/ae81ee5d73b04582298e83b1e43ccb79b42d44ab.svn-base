.AM_albumUploadWarp{width:970px; margin:0 auto;}
.AM_albumUploadWarp h4{ height:30px; line-height:29px; color:#7e7e7e;}
.albumUploadSetmod dl{ padding:5px 0;}
/*=====MyactivitySet=====*/
.MyactivitySet_Widget{width:248px; height:auto; padding-bottom:30px;}
.MyactivitySet_title{height:29px; line-height:28px;color:#7e7e7e;border-top:1px solid #1581a7;background:#eef7fe; padding:0 10px;}
.MyactivitySet_Main{ width:705px; height:auto;overflow:hidden; padding:0 0 0 15px;padding-bottom:30px; }

.activityList_Will li{ border-bottom:1px dotted #1581a7; padding:10px 10px 10px 20px; position: relative;}
.activityList_Will h3{ cursor: pointer;}
.activityList_Will .back{ position: absolute; right:15px;bottom:10px;}
.activityList_Will .switchover{ background:#f7f7f7;}
.activityList_Will .activityDetailList{ display:none;}
.activityList_Will .switchover .activityDetailList{ display:block;}

.activityDetailList li{ border:none; padding:0 0 0 18px; background:url(../images/icons/listType.png) left center no-repeat;}
.setForm { width:690px; margin:0 auto; color:#7e7e7e;}
.setForm dl{ padding:0 20px;line-height:33px;}
.setForm dd{ padding:10px 0;}
/*=====timeline=====*/
.timelineWarp{ width:718px; margin:10px auto; position:relative;}
.timelineTop{height:30px; position:relative; display:block; overflow:hidden;}
.tips_B{ position:absolute; display:block;width:79px; height:28px; background:url(../images/timeline/bg_richeng_showdate.png) no-repeat; line-height:19px; text-align:center;}
.timelineBtm{height:80px; position:relative; overflow:hidden; margin:8px 0 0 0;}
.timelineBtm .tipsWarp{ width:236px; height:auto; position:absolute;}
.timelineBtm .tipsWarp .tipsTop{ width:236px; height:10px; background:url(../images/timeline/bg_richeng_showDetail.png) no-repeat 0 0;}
.timelineBtm .tipsWarp .tipsMain{ width:234px; height:auto; background:#f6fbfe; border-left:1px solid #72b5c8;border-right:1px solid #72b5c8;}
.timelineBtm .tipsWarp .tipsMain p{ width:200px; height:24px; line-height:23px; margin:0 auto; }
.timelineBtm .tipsWarp .tipsBottom{ width:236px; height:10px; font-size:0; background:url(../images/timeline/bg_richeng_showDetail.png) no-repeat 0 -10px; display:block;}
.timelineMain{ width:700px; height:24px; line-height:23px; background:url(../images/timeline/bg_richeng.png) left  center no-repeat; padding:0 14px;}
.timelineMain li{float:left; background:url(../images/timeline/bg_richeng_round.png) left center no-repeat; width:20px; height:21px; margin:2px 14px 0 15px;}
.timelineMain li.current{ width:23px; height:24px; background:url(../images/timeline/bg_richeng_cur.png) left center no-repeat; margin:0 12px 0 12px;}
/*=====harvestPay=====*/
.harvestPayWarp{ background:#f7f7f7; padding:7px 7px;}
/*memberList*/
.memberList{ background:#f9f9f9;}

.list-opt{ position:relative;width:44px; height:24px;text-align:right;}
.list-opt .opt-button{border:1px solid #fff; height:24px; position:relative; z-index:2; background:#fff;}
.list-opt span{width:16px; height:16px; margin:2px auto; background:url(../images/icons/arrow_small.png) no-repeat -16px 0;}
.list-opt .selectMain{width:90px; height:auto; line-height:24px; text-align:right; padding:0 5px; border:1px solid #e0e0e0; z-index:1;top:24px; right:0; background:#fff; overflow:hidden; display:none;}
.list-opt:hover{ z-index:10;}
.list-opt:hover .opt-button{border-top:1px solid #e0e0e0;border-left:1px solid #e0e0e0;border-right:1px solid #e0e0e0;border-bottom:1px solid #fff;}
.list-opt:hover .selectMain{ display:block;}
.pagewrap{ margin-left:20px;}
.pagewrap .pageList{ margin:5px 0;}
/*====Road Line====*/
.roadLine{ width:690px;margin:0 auto; position:relative;}
.rodalineTop{width:690px; height:24px; line-height:23px; position:relative; margin:0 auto;}
.rodalineTop span{ width:auto; height:24px; line-height:23px; position:absolute;}
.rodalineMiddle{width:690px; height:24px; line-height:23px; position:relative;}
.roadlineLeft,.roadlineRight{width:10px;height:24px;}
.roadlineLeft{left:0; background:url(../images/roadline/roadlineL.png) left center no-repeat; float:left;}
.roadlineRight{right:0;background:url(../images/roadline/roadlineR.png) right center no-repeat;float:right;}
.roadlineCenter{width:670px;height:24px; position:relativel;background:url(../images/roadline/roadlineC.png) left center repeat-x; float:left;}
.roadlineCenter .selected{ width:42px; height:18px; background:url(../images/roadline/selected.png) left center no-repeat; display:block; position:absolute;  left:19px; z-index:1;}
.roadlineCenter li{ position:relative;}
.roadlineCenter li .tipsWarp{ left:0; top:16px;left:22px;}
.roadlineCenter li:hover .tipsWarp{ display:block;}
.roadlineCenter .star{ width:25px; height:24px; background:url(../images/roadline/star.png) left center no-repeat; float:left; margin:0;}
.roadlineCenter .end{ width:25px; height:24px; background:url(../images/roadline/end.png) left center no-repeat; float:right; margin:0;}
.roadlineCenter li{ width:15px; height:14px; margin:5px 8px 0 8px; float:left; background:url(../images/roadline/node.png) left  center no-repeat; display:block;}
.rodalineBottom{ width:auto; height:24px;;position:relative;}
.rodalineBottom .tipsWarp{ width:236px; height:auto; position:absolute; left:98px; display:none; }
.rodalineBottom .tipsWarp .tipsTop{ width:236px; height:10px; background:url(../images/timeline/bg_richeng_showDetail.png) no-repeat 0 0;}
.rodalineBottom .tipsWarp .tipsMain{ width:234px; height:auto; background:#f6fbfe; border-left:1px solid #72b5c8;border-right:1px solid #72b5c8;}
.rodalineBottom .tipsWarp .tipsMain p{ width:200px; height:24px; line-height:23px; margin:0 auto; }
.rodalineBottom .tipsWarp .tipsBottom{ width:236px; height:10px; font-size:0; background:url(../images/timeline/bg_richeng_showDetail.png) no-repeat 0 -10px; display:block;}
.starText{float:left; background:url(../images/roadline/starText.png) top center no-repeat; padding:19px 0 0 0;text-align:center; margin:5px 0 0 5px; color:#666;}
.endText{float:right; background:url(../images/roadline/endText.png) top center no-repeat;padding:19px 0 0 0; text-align:center;  margin:5px 3px 0 0 ;color:#666;}
/*map*/
/*.roadLineMap{margin:10px auto; *+margin:20px auto;}*/
.linemapL,.linemapR{ float:left;}
.linemapL{width:458px; height:398px; border:1px solid #ededed;}
.linemapR {width:220px; margin:0 0 0 10px;}
.linemapR .memberlineList{ height:auto; border-bottom:1px solid #1992b1; }

.linemapR .memberlineList .tabs-clip{ overflow: hidden;width:170px;display: inline-block; *display: inline;zoom:1;overflow:hidden; vertical-align: middle;}
.linemapR .memberlineList li{ float:left; margin:0 3px 0 3px; height:24px;line-height:23px;}
.linemapR .memberlineList .selected{ border-bottom:3px solid #1992b1}
.linemapR .roadlinemapStar{width:168px; color:#7d7d7d ;padding:0 0 0 50px; height:40px; border:1px solid #bbb; background:#f9f9f9 url(../images/roadline/liststar.png) left center no-repeat; line-height:41px;}
.linemapR .roadlinemapEnd{width:168px; color:#7d7d7d ; padding:0 0 0 50px; height:40px; border:1px solid #bbb; background:#f9f9f9 url(../images/roadline/listend.png) left center no-repeat;  line-height:41px;}
.linemapR .total{line-height:40px; border-bottom:1px dashed #bbb;}
.linemapR .sublineList{ width:220px; height:284px; overflow-y:scroll; overflow-x:hidden;}
.linemapR .sublineList li{width:203px; border-bottom:1px dashed #bbb; line-height:24px; padding:5px 0 5px 0; color:#7d7d7d; overflow:hidden;}
.linemapR .sublineList li span{ color:#1383a9;}
.linemapR .moveL,.linemapR .moveR{width:16px; height:16px; display:inline-block; cursor:pointer; vertical-align: middle; }
.linemapR .moveL{ background:url(../images/roadline/leftarrow.png) left  center no-repeat;}
.linemapR .moveR{  background:url(../images/roadline/rightarrow.png) left center  no-repeat; }
