<script type="text/javascript">
    /*
    *   message list box  javascript;
    */
	$('.replay-content .icon-packup').click(function(){
		$(this).closest('.replay-content').stop(true,true).animate({ height: 'hide'});
	})
    $('.msglistbox .msgreplay').click(function(){
        $(this).closest('li').find('.replay-content').stop(true,true).animate({ height: 'show'});
    })

    /*
    *  textarea focus show;
    */
    $(".replay-content .textarea").focus(function(){
        if(!$(this).attr('data-height')){
            $(this).attr('data-height',$(this).height())
        }
        $(this).stop().animate({height: 100 },'fast');
    }).blur(function(){
        $(this).stop().animate({height:$(this).attr('data-height')},'fast');
    });

    $('.replay-content .replay-btn').click(function(){
        var $content = $(this).closest('.replay-content');
        var $textarea = $content.find('.replay-textarea').children();
        $content.data('textarea',$textarea);
        if($textarea)
            $textarea.appendTo($(this).closest('.overflow').find('.replay-textarea'));
    });

    $('.msgreplay .replay-btn').click(function(){
        var $content =  $(this).closest('.msgconent').find('.replay-content');
        var $textarea = $content.data('textarea');
        $textarea.appendTo($content.find('.replay-textarea:eq(0)'));
    })


    /*
     *  end  message list box  javascript;
     */
</script>
