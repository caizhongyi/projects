/* CSS Document */
/*=====Global=====*/
table{border-collapse:collapse;border-spacing:0;}
/*====method=====*/
.table-fixed{table-layout:fixed;}
.table-auto{table-layout:auto;}
.addTable tr{ height:34px; line-height:33px;}
.addTable td{ padding: 0 3px;}
.preparedList { margin:5px auto;}
.preparedList tr{ height:24px; line-height:23px;}
.preparedList .table-opt{ visibility:hidden;}
.preparedList tr:hover{background:#EDEDED}
.preparedList tr:hover .table-opt{ visibility:visible;}
.preparedList th{ font-weight:normal; text-align:left; padding:0 8px;}
.preparedList td{ padding:0 8px; line-height:30px;}
.preparedList .listDel{ visibility:hidden;}
.preparedList tr:hover .listDel{ visibility:visible;}
/*====harvestPayTable====*/
.harvestPayTable{}
.harvestPayTable tr{ height:26px;}
.harvestPayTable th{text-align:left; font-weight:normal;color:#1582A5; height:38px;}
.harvestPayTable td{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;color:#7e7e7e;border-bottom:1px dashed #bbb; line-height:35px;}
.harvestPayTable .nCol{background:transparent;}
.subTableWarp{position:relative;}
.subTable {margin:3px 0px;}
.subTable td{white-space:nowrap; *+white-space:pre;text-overflow:ellipsis;overflow:hidden;border:none; background:#ededed;}
.subTableWarp ins{width:6px; height:15px; display:inline-block; background:url(../../images/icons/link.png) no-repeat; position:absolute; top:20px; left:10px;_top:23px;}
.harvestPayTable .listEdit,.harvestPayTable .listDel{ display:none;}
.harvestPayTable tr:hover{ background:#ededed;}
.harvestPayTable tr:hover .listEdit,.harvestPayTable tr:hover .listDel,
.harvestPayTable .nCol .listEdit,.harvestPayTable .nCol .listDel{ display:block;}
/*memberTable*/
.memberTable{}
.memberTable tr{ height:28px;}
.memberTable td{ border-bottom:1px dashed #bbb;}
.memberHead{border:1px solid #e5e5e5;}
.submited{color:#0965ae;}
.nosubmit{color:#e31822;}
.roleName{color:#ea7730;}
.opera{width:auto; height:auto;/*CSS3*/ padding:0 0 5px; position:relative; }
.operaselect{ border:1px solid #bbb;background:#fff;}
.opera a:hover{text-decoration:none;}
.operaicon{width:40px; height:28px; background:url(../../images/icons/icon_opera.png) center no-repeat; display:inline-block;}
.operaList {position:absolute; top:32px; left:-1px; width:40px;margin:0 auto; background:#fff; border:1px solid #bbb; border-top:none; display:none;}
.operaList li{ width:28px; margin:0 auto; border-bottom:1px dotted #bbb; text-align:center;}
.memberFoot{border:1px solid #e5e5e5;}


/* ==============================  update by cai ===================================== */
.mes_com_box_Tab {
    height:24px;
    line-height:24px;
    border-bottom:1px solid #8CC6D6;
}
.mes_com_box_Tab a {

    margin:0 8px 0px 0px;
    text-align:center;
    color:#636466;
    display:inline-block;
    height:100%;

    padding:0 35px;
    border-radius:5px 5px 0 0;
    -moz-border-radius: 5px 5px 0 0;
    -webkit-border-radius: 5px 5px 0 0;
    background-color:#B6DFEB;
    border-color:#A5D3E0;
    border-width:1px 1px 0;
    border-style:solid;

}
.mes_com_box_Tab .active,
.mes_com_box_Tab a:hover {
    color:#fff;
    background-color:#1683A7;
    border:1px solid #3187A1;
    font-size:12px;
    text-decoration:none;
    border-bottom:none;
}
.mes_small_box_tab{ margin:10px 0;}
.mes_small_box_tab a{ width:60px;  padding:0;  }

/*  tabs  */
.tabs{ margin:20px auto; }
.tabs .tabs-nav{ margin-left:20px;font-size:12px;  }
.tabs .tabs-nav,
.tabs .tabs-nav a{ color:#1992b1; }
.tabs .tabs-nav .current,
.tabs .tabs-nav li:hover
{ background:#1992b1;color:#fff;}
.tabs .tabs-nav li:hover a,
.tabs .tabs-nav .current a{color:#fff;}
.tabs .tabs-nav a:hover{ text-decoration:none;}
.tabs .tabs-nav li{background:#fff; margin: 0 5px;}
.tabs-color .tabs-nav li{ background:#b6e0ea; border:1px solid #9ecfdc; border-bottom:none; }
.tabs .tabs-nav li,
.tabs-color .tabs-nav li{display:inline-block; *display:inline; zoom:1; padding:5px 40px; border-radius:5px 5px 0 0;-moz-border-radius: 5px 5px 0 0;-webkit-border-radius: 5px 5px 0 0; color:#666; }
.small-tabs-color .tabs-nav li{padding:5px 25px;}
/*.tabs .tabs-panel{ border-top:1px solid #9ecfdc;}*/

.tabs-color  .tabs-nav ,.small-tabs-color  .tabs-nav { margin-left:0;}
.tabs-color > li:first-child,.small-tabs-color > li:first-child{ margin-left:0;}

.sub-tabs{}
.sub-tabs .sub-tabs-nav li{display:inline-block; *display:inline; zoom:1; margin-right:10px;}
.sub-tabs .sub-tabs-nav li:hover
.sub-tabs .sub-tabs-nav li.current{ border-bottom:3px solid #1992b1;}
/*  $tabs  */

/* tips */
/*  */
.tips{ font-size: 12px; line-height: normal; border:1px solid #c7ddea; border-radius:4px; -moz-border-radius:4px;-webkit-border-radius:4px;padding:10px; background:#ffffe5;}
.tips .close-3{  position:absolute; right:10px; top:10px; }
.tips .arrowhead{ position:absolute; left:-11px; top:25%; background:url(../../images/arrowhead.png) no-repeat; width:12px; height:18px; z-index:2;}
.tips .down-arrowhead{ position:absolute; bottom:-9px; left:15%; background:url(../../images/arrowhead_down.png) no-repeat; width:15px; height:10px; z-index:1;}
.tips .up-arrowhead{ position:absolute; top:-13px; left:15%; background:url(../../images/arrowhead_up.png) no-repeat; width:15px; height:15px; z-index:1;}
.tips-panel{position:relative; display:inline-block; *display:inline; zoom:1;}
.tips .form li{ margin:5px 0;}
.tips p{ color:#F79646;}

.tips-1{border:1px solid #e5e5e5;  padding:10px; background:#f7f7f7; position:relative;}
.tips-1 .arrowhead{ position:absolute; top:-7px; left:15%; background:url(../../images/arrowhead_1.png) no-repeat; width:13px; height:7px; z-index:1;}

.messagetip{ position:absolute;  right:180px; top:20px;}
.messagetip > span{ position:relative; }
.messagetip .tip-num{ color:#E7681B; position:absolute;display:block; right:-23px; top:-20px; background:url(../../images/bg_msg.png)  no-repeat; width:30px; height:28px; text-align:center;}

/* star */
.star{ background:url(../../images/star.png) repeat-x; display:inline-block; vertical-align:middle; height:12px;  }
.star1{width:12px;}
.star2{width:24px;}
.star3{width:36px;}
.star4{width:48px;}
.star5{width:60px;}


/* search text*/
.search-textbox input[type=text]{ background:#fff; border:1px solid #ddd; height:22px; line-height:22px; }
.search-textbox input[type=button],
.search-textbox input[type=submit]{ background:#e9e9e9 url(../../images/search_result.png) no-repeat;width:33px; height:24px; border:none; border:1px solid #ddd; cursor:pointer;}


/*
	message list content
*/
.msglistbox{
    color: #7D7D7D;
    height: auto;
    margin: 0 auto;
}
.msglistbox li.selected,
.msglistbox li:hover{
    background: none repeat scroll 0 0 #F9F9F9;
}
.msglistbox li {
    border-bottom: 1px dashed #BABABA;
    height: auto;
    padding: 10px;
    position: relative;
}
.msglistbox .photo{float:left; margin-right: 10px; width: 48px; height: 48px; }
.msglistbox .msgconent{ overflow: hidden;}
.msglistbox .mbUsername { color: #EA7730; margin-bottom: 5px;}
.msglistbox .msgdate{ margin-top: 5px;}
.msglistbox .msgreplay{ display: block;margin-right: 10%; }
.msglistbox .replay-content .arrowhead{right:10%;}
/*
	relay list content
	回复列表
*/
.replay-content{ background:#f9f9f9; padding:20px; border:1px solid #d8d8d8; position:relative; margin-top:10px; color:#666;}
.replay-content .arrowhead{ border: none; position:absolute; top:-9px; right:10%; background:url(../../images/home/ico_17x9.png) no-repeat; width:17px; height:9px;}
.replay-content .text,
.replay-content .textarea
{ width:400px;  max-width: 400px; }
.replay-content .textarea{height:19px; vertical-align: top; }
.replay-content .mbUsername { color: #EA7730;margin-bottom: 5px;}
.replay-content .photo{ float:left; margin-right: 10px; width: 48px; height: 48px; }
.replay-content .button{  padding:4px 10px;}
.replay-content .button2{margin:0 10px; color:#1d74b7;}
.replay-content .submit{color:#1d74b7; font-weight:normal; border:none;}
.replay-content .cancel{color:#666; font-weight:normal; border:none;}
.replay-content ul li{ border-bottom:1px dotted #ddd; padding:10px 0;}
.replay-content li:hover{ background:#f9f9f9;}
.replay-content p{  display:block; line-height:22px; }
.replay-content .replay{ float:right;}
.replay-content .replay a{ margin:0 5px;}
.replay-content .showall{ text-align:right;}
.replay-content .time{ float:left; color:#999; }
.replay-content .time,
.replay-content .replay{margin: 5px 0;}
.replay-content .showall a{margin-top:10px; display:inline-block;}
