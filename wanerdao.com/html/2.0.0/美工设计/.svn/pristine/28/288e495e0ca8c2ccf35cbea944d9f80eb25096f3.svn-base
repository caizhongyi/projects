/*=====Global=====*/
input,textarea,select{vertical-align:middle;}
.form{ font-size:14px; color:#bcc4c5;}
.form .label{ display:inline-block; margin-right:10px; vertical-align:middle; }
.form h3{ font-weight:bold; color:#333;}
.form h3 span{  margin:0 5px; font-size:12px; font-weight:normal;}
.form .wraplabel{ color:#333;font-size:12px;}
.form .wraplabel span{ color:#666666;}
.form .wraplabel span,.form .wraplabel a ,.form .wraplabel .text { margin-left:5px;}
/* select 修正 */
.form .wraplabel .chzn-single{ margin-left:0;}
.form .wraplabel .chzn-container{ margin-left:5px;}
/* select 修正 */
.form .text{ vertical-align:middle;}
.form li{ margin:15px 0; text-align:left;}
.form .submit{ text-align:center; margin:30px 0;}
.form .submit .button{ margin:0 10px;}
.form .result{ text-align:center; margin:30px 0;}


/*  textboxs  */

.text{ padding-left:5px;}
.text,.select,.chzn-container{ height:25px; line-height:25px; display:inline-block;  }
.text,.textarea,.labeltext,.select{border:1px solid #dfdfdf; border-radius:3px;-moz-border-radius: 3px;-webkit-border-radius: 3px;line-height:23px;background:#fff; box-shadow:inset 0 0 4px #dfdfdf; }
.textarea{padding:5px;}
/* text font */
