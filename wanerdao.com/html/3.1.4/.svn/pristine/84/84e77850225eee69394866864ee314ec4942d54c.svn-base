@charset "utf-8";
body {
    font: 14px/1.6 "宋体", arial;
    background: #ffffff url(../images/bg.png);
    color: #666666;
    min-width: 320px;
}

.main {
    display: block;
    overflow: hidden;
}

.relative {
    position: relative;
}

.h1 {
    font-size: 36px;
}

.h2 {
    font-size: 30px;
}

.h3 {
    font-size: 24px;
}

.h4 {
    font-size: 18px;
}

.h5 {
    font-size: 16px;
}

.h6 {
    font-size: 14px;
}

h3 .text-inherit {
    color: inherit;
}

img {
    max-width: 100%;
    vertical-align: middle;
    border: 1px solid #eee;
}

img.bordered {
    box-shadow: 1px 1px 5px #ddd;
    background: #fff;
    padding: 4px;
}

img.transparent {
    background: none;
    border: none;
}

a {
    color: #1992b1;
}

p + p,
.section + .section {
    margin-top: 10px;
}

.block {
    display: block;
}

.block + .block {
    margin-top: 10px;
}

.inline,
.inline-block {
    display: inline-block;
    vertical-align: middle;
}

.inline + .inline {
    margin-left: 10px;
}

.bordered {
    border: 1px solid #ddd;
    padding: 10px;
}

.scrollfixed.fixed {
    top: 0;
}

.summary,
.address {
    font-size: 12px;
}

.page-header {
    font-size: 16px;
    border-bottom: 1px dotted #ddd;
    margin-bottom: 15px;
    padding: 10px 0;
}

.time,
time,
.desc {
    font-size: 12px;
    color: #999;
}

.address,
address,
.text-dark {
    color: #333;
}

.desc {
    text-indent: 0;
}

.fixed {
    position: fixed !important;
    z-index: 50;
}

.box{
    border: 1px solid #9ecfdc;
}
.box.fixed{
    background: #fff;
    width: 250px;
    right: 50%;
    top: 20%;
    margin-right: -500px;
    z-index: 1;
}
.box-inner{
    padding: 25px;
}

.text-indent {
    text-indent: 2em;
}

.text-info {
    color: #6cafbb;
}

.caption-mini {
    font-size: 12px;
    margin-bottom: 10px;
}

.caption-mini .caption-side {
    float: left;
    padding-top: 5px;
}

.caption-mini .input-search {
    margin: 2px;
    float: right;
}

/*strong,.small,small,span , a{ vertical-align: middle;}*/
small,
.small {
    font-size: 12px;
    color: #999;
}

div.small {
    margin: 0;
    display: block;
}

.heading {
    margin-bottom: 5px;
}

.title {
    font-size: 14px;
}

.title small,
.title .small {
    margin-left: 5px;
    font-size: 12px;
}

.title .time.pull-right {
    margin-top: 3px;
}

.date {
    font-size: 12px;
    color: #999;
}

.price,
.text-important,
.text-danger {
    color: #ce0808 !important;
}
.text-black{
    color: #000;
}

.opts {
    position: absolute;
}

.opts a + a,
.opts a + span,
.opts span + a {
    margin-left: 5px;
}

.opts [class*="icon"] {
    margin-left: 0;
}

.static {
    position: static !important;
}

.table caption {
    padding: 5px 10px;
}

.table-bordered th,
.table-bordered td {
    border: 1px solid #dddddd;
}

.table-col-6 th {
    width: 17%;
}

.layout-main {
    overflow: hidden;
    *zoom: 1;
    _display: inline;
}

.scroll,
.scroll-x,
.scroll-y {
    position: relative;
}

.scroll .scroll-panel,
.scroll-x .scroll-panel,
.scroll-y .scroll-panel {
    overflow-y: auto;
    overflow-x: hidden;
    position: relative;
    max-height: 80px;
}

.scroll-x .scroll-panel {
    overflow-x: auto;
    overflow-y: hidden;
}

.scroll-xy {
    position: relative;
}

.scroll-xy .scroll-panel {
    overflow: auto;
}

.scroll-high .scroll-panel {
    max-height: 150px !important;
}

.scroll-higher .scroll-panel {
    max-height: 200px !important;
}

.hover {
    position: relative;
}

.hover .hover-panel,
.hover .panel {
    visibility: hidden;
    opacity: 0;
    transition: all 0.2s ease-in-out 0s;
    position: absolute;
    display: block;
}

.hover:hover .hover-panel,
.hover:hover .panel {
    visibility: visible;
    opacity: 1;
}

.hide {
    display: none;
}

.animation-opts .opts {
    opacity: 0;
    visibility: hidden;
    transition: all 0.2s ease-in-out;
    transform: scale(0);
    -webkit-transform: scale(0);
}

.animation-opts li:hover .opts {
    opacity: 1;
    visibility: inherit;
    transform: scale(1);
    -webkit-transform: scale(1);
}

/*  badge  */
.badge {
    background-color: #999999;
    border-radius: 10px;
    color: #FFFFFF;
    display: inline-block;
    font-size: 12px;
    font-weight: bold;
    line-height: 1;
    min-width: 10px;
    padding: 3px 7px;
    text-align: center;
    vertical-align: baseline;
    white-space: nowrap;
}

.badge-important {
    background-color: #D15B47;
}

.badge-important.badge-transparent {
    color: #D15B47;
    background: none;
}

/* end  badge  */
