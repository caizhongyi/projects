        margin: 10px 0;
    }
    .header .search .text {
        width: 70%;
    }
    .header > .container {
        padding: 10px;
    }
    .header .userinfo{
        float: none;
    }
    .ui-form li > .label {
        text-align: left;
        display: block;
    }
    .ui-form .label + .tips-result{
        margin: -10px 0 0 0;
    }
    .ui-form li .text,.ui-form .btn {
        margin-left: 0;
    }
    .ui-form .btn { display: block; margin-bottom: 10px;}
    .form [class*="input-label"]{ display: block; text-align: left; margin-bottom: 10px;}
    .form [class*="input-label"] + .text{ margin-left: 0; width: 250px;}

    .form [class*='form-panel']{ padding-left: 0;}
    .form .combo{
        margin-left: 0 !important;
        display: block;
        width:150px !important;
        margin-bottom: 10px;
    }

    .ui-tabs{
        margin-top: 5px;
    }
    .ui-tabs .panel {
        padding: 20px;
    }
    .ui-tabs .nav a{display: block;}
    .ui-tabs .nav a.selected{
        border:none;
    }
}

@media (max-width:500px) {
    .pwd-security{ margin: 10px 0; display: block;}
    .slider-about .slider-item{ font-size: 1em ;padding-top:0;}
    .slider-about{ height: 350px;}
    .slider .next, .slider .prev{ font-size: 2em;}
    .slider .next{ right: 10px;}
    .slider .prev{ left: 10px;}
}

