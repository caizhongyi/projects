            function pageLastClick(){
                var $chartBoardpagination = $('.chartBoardpagination');
                var $lis = $chartBoardpagination.find('dd');
                $lis.filter(':gt(4)').hide();
                $lis.filter(':lt(5)').addClass('active');
                var total_len=$('#mylst dd').length;

                var current_len=$('.chartBoardpagination > dd:visible').length;
                var lenn=total_len/current_len;
                var split_values=lenn.toString().split(".");
                var t_split=split_values[0];
                var sub=lenn-t_split;
                var mul=sub*5;
                var lst_split=mul.toString().split('.');

                var len1=Math.round(total_len/current_len);
                $("#t_leng").text(len1);
                $("#t_leng1").text(len1);

                $lis.filter('.active').removeClass('active').hide();
                $lis.slice(-lst_split[0]).addClass('active').show();
            }

