    function savePrint()
    {

        if(document.getElementById('dropid').value=="PDF"){
            var module = document.printForm;
            var txt = "";
            var i;
            for (i = 0; i < module.length; i++) {
                if (module[i].checked) {
                    txt = txt + module[i].value + ",";
                }
            }
            //Ajax call made to write module data to PDF
            var today = new Date();
            var dd = today.getDate();var mm = today.getMonth()+1; //January is 0!
            var yyyy = today.getFullYear();
            if(dd<10) {
                dd='0'+dd;
            }if(mm<10) {
                mm='0'+mm;
            } today = +yyyy+mm+dd;
            var activityId=document.getElementById("activityId").value;
            var activityName="_"+today;
            $.ajax({
                type : "GET",
                url : contextPath + "/writeDataToPDF",
                data : "selectedModules="+txt +"&activityId="+activityId+"&activityName="+activityName,
                cache : false,
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                async : false,
                success : function(response)
                {
                    if(response=="Success"){
                        downloadPDFReport();
                    }
                },
                error : function(e) {
                    //	 alert('Error: ' + e.responseText);
                }
            });
        }
    }
    //To open the saved PDF
    function downloadPDFReport() {
        var today = new Date();
        var dd = today.getDate();var mm = today.getMonth()+1; var yyyy = today.getFullYear();
        if(dd<10) {
            dd='0'+dd;
        }if(mm<10) {
            mm='0'+mm;
        }
        today = +yyyy+mm+dd;
        var activityId=document.getElementById("activityId").value;
        var activityName="_"+today;
        var url=contextPath+"/downloadPDFReport.do?activityId="+activityId+"&activityName="+activityName;
        window.open(url,'','width=700,height=500');
    }
