                                    function calculatebetweenDates(){
                                        var testEndDate=document.getElementById("todate").value;
                                        var testStartDate=document.getElementById("fromdate").value;

                                        if(testStartDate != '' && testEndDate != '' && testStartDate != null && testEndDate != null){
                                            var startdateval =testStartDate.split(' ');
                                            var enddateval =testEndDate.split(' ');

                                            var date1 = new Date(startdateval);
                                            var date2 = new Date(enddateval);
                                            //var timeDiff = date2.getTime() - date1.getTime();
                                            var oneDay = 24 * 60 * 60 * 1000;
                                            //var diffDays = timeDiff / (1000 * 3600 * 24);
                                            var diffDays = Math.round(Math.abs((date1.getTime() - date2.getTime()) / (oneDay)));

                                            /* 	// hours*minutes*seconds*milliseconds
                                             var firstDate = new Date(date1.getYear(), date1.getMonth(), date1.getDay());
                                             var secondDate = new Date(date2.getYear(), date2.getMonth(), date2.getDay());
                                             //alert(firstDate+"=="+secondDate);
                                             var diffDays = Math.round(Math.abs((firstDate.getTime() - secondDate.getTime()) / (oneDay))); */

                                            $('#betweendate').show();
                                            if(diffDays>0){
                                                if(diffDays < 10)
                                                {
                                                    document.getElementById("betweendate").innerHTML="0"+diffDays+"&nbsp; days diff";
                                                }else{
                                                    document.getElementById("betweendate").innerHTML=diffDays+"&nbsp; days diff";
                                                }
                                            }else{
                                                document.getElementById("betweendate").innerHTML="";
                                            }

                                        }
                                    }

