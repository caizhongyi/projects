                    tooltip : {
                        visible : true,
                        format : "{0}",
                        template : templatedata
                    },
                    legendItemClick : onBehaviourLegendClick
                });
        var chart = $("#activitygraph").data("kendoChart");

        var series = chart.options.series;
        // set series colors
        if(typeof series != "undefined"){
            for(i=0; i<series.length; i++)
            {
                if(i==0){
                    series[i].color='red';
                }else if(i==1){
                    series[i].color='blue';
                }
            }
        }
        chart.refresh();
    }



