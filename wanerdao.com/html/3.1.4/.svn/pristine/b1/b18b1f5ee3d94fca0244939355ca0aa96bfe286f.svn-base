        var texttitle="";
        var categoryTextBehaviour="";
        var notesdata="";
        $("#activitygraph").kendoChart(
                {
                    title : {
                        text : "Activity Survey Column Graph"
                    },
                    legend : {
                        position : "right"
                    },
                    chartArea : {
                        background : ""
                    },
                    seriesDefaults : {
                        type : "column",
                        markers: {
                            visible: true,
                            type: "square",
                        },
                        missingValues: 'gap'
                    },
                    series : chartData,
                    valueAxis : {

                        title: {
                            text: texttitle,
                            margin: {
