                        var moduledetails = parsedJson.getDetailsAboutModuleslist[i];
                        var imagesrc = "";
                        imagesrc = imagesrc+moduledetails[6];
                        var content = "<a onclick='testfunctionback();' id='back' class='btn btn-default btn-mini' style='margin-top:10px;margin-left:10px;width:62px;'>back</a><div class='module-info media'>";
                        if(moduledetails[1] == 1)
                        {
                            content = content + "<img src='"+imagesrc+"' style='width: 75px;' class='media-object'>";
                        }
                        else{
                            content = content + "<img src='"+imagesrc+"' style='width: 75px;' class='media-object'>";
                        }
                        content = content + "<div class='media-body'> <h4>"+moduledetails[2]+"</h4><div>"+moduledetails[5]+"</div>";

                        if(moduledetails[4] == 0){
                            if(moduledetails[1] == 0){
                                content = content + "<div class='opts'><a onclick=installfunction("+moduledetails[0]+");>Install</a></div>";
                            }
                            else
                            {
                                content = content + "<div class='opts'><a onclick=deletemoduletemp("+moduledetails[0]+");>Uninstall</a></div>";

                            }
                        }
                        content = content + "</div><div class='clear block'></div>";
                        content = content + "<div class='desc block'>"+moduledetails[3]+"</div></div>";




                        $('#moduleinfo').append(content);


                    }

                }
            },

        });


    }

    function installfunction(moduleid)
    {

        $('#moduelist').show();
        $('#moduleinfo,#back').hide();
        $("#installmodulepart").hide();
        $("#installmodulepartdiv").hide();
        $("#installmodulepartdynamic").children().remove();
        $("#installmodulepartdynamicdiv").show();
        $("#installmodulepartdynamic").show();
        $("#count").children().remove();
        $.ajax({
            type : "POST",
            url : contextPath + "/insertmoduletempforfuture?moduleid="+moduleid,
            cache : false,
            async : false,
            dataType: "json",
            success : function(response) {
                var parsedJson = response;

                if(parsedJson != '')
                {
                    $("#count").append("<em>"+parsedJson.length+"</em>");
                    for(var i=0; i<parsedJson.length;i++)
                    {

                        var allInstalledModulesList = parsedJson[i];
                        var imagesrc = "";
                        imagesrc = imagesrc+allInstalledModulesList[8];

                        var string1 = allInstalledModulesList[3];
                        var string2 = string1.replace("Activity", "");
                        var string3 = string2.replace("module", "");
                        var content ="<li style='width:40%;'><div class='col-inner'><div class='thumb'>";
                        if(allInstalledModulesList[4] == false)
                        {
                            content = content +"<img  src='"+imagesrc+"'  style='border-style:solid;border-width: 1px;border-color:red;'><h6 id='textmodulename'>"+string3+"</h6>";
                        }
                        else
                        {
                            content = content +"<img  src='"+imagesrc+"'><h6 id='textmodulename'>"+string3+"</h6>";
                        }

                        if(allInstalledModulesList[4] == false)
                        {
                            content = content+"<div class='opts'><a title='uninstall' onclick=deletemoduletemp("+allInstalledModulesList[6]+"); class='icon icon-del'></a></div>";
                        }
                        else
                        {
                            /* content = content+"<div><a title='uninstall' style='opacity:0.2;' class='icon icon-del'></a></div>"; */
                        }
                        content = content + "</div></div></li>";
                        $('#installmodulepartdynamic').append(content);



                    }

                }
            },

        });
    }



</script>
