            function pageNextClick(){
                var $chartBoardpagination = $('.chartBoardpagination');
                var $lis = $chartBoardpagination.find('dd:not(#latest_prev, #latest_next)');
                var idx = $lis.index($lis.filter('.active:last')) || 0;
                var $toHighlight = $lis.slice(idx + 1, idx + 6);
                $("#latest_prev").removeClass("disabled");
                document.getElementById('latest_prev').style.pointerEvents = '';
                if ($toHighlight.length<5)
                {
                    $("#latest_next").addClass("disabled");
                    document.getElementById('latest_next').style.pointerEvents = 'none';
                }
                if ($toHighlight.length == 0)
                {
                    $("#latest_prev").show();
                    $("#latest_next").addClass("disabled");
                    return;
                }
                $("#latest_next").show();
                $lis.filter('.active').removeClass('active').hide();
                $toHighlight.show().addClass('active');

            }
            function pagePreClick(){
                var $chartBoardpagination = $('.chartBoardpagination');
                var $lis = $chartBoardpagination.find('dd:not(#latest_prev, #latest_next)');
                var idx = $lis.index($lis.filter('.active:first')) || 0;
                var start = idx < 5 ? 0 : idx - 5;
                var $toHighlight = $lis.slice(start, start + 5);
                $("#latest_next").removeClass("disabled");
                document.getElementById('latest_next').style.pointerEvents = '';
                if ($toHighlight.length == 0)
                {
                    $("#latest_prev").hide();
                    return;
                } else
                {
                    $lis.filter('.active').removeClass('active').hide();
                    $toHighlight.show().addClass('active');
                }
                if(idx <6)
                {
                    $("#latest_prev").addClass("disabled");
                    document.getElementById('latest_prev').style.pointerEvents = 'none';

                }else{
                    document.getElementById('latest_prev').style.pointerEvents = '';
                }

            }

            function pageFirstClick(){
