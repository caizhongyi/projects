    function quitSubmitAdmin()
    {
        var rightPanelHtml = $(".rightpanel").html().trim();
        var reasonAdmin = $('#reasonAdmin').val().trim();
        if(rightPanelHtml == "")
        {
            $('#errorMember').show();
        }
        else if(reasonAdmin == "")
        {
            $('#errorreason').show();
        }
        else
        {
            activityId = document.getElementById("activityId").value;
            memberId = document.getElementById("memberId").value;
            document.quitFormFuture.action = contextPath + "/quitActivity?activityId="+activityId+"&memberId="+memberId+"&reason="+reasonAdmin;
            document.quitFormFuture.submit();
        }
    }

    function quitSubmitMember()
    {
        var reasonMember = $('#reasonMember').val().trim();
        if(reasonMember == "")
        {
            $('#errorreason1').show();
        }
        else
        {
            activityId = document.getElementById("activityId").value;
            document.getElementById("reason").value = reasonMember;
            document.quitFormFuture.action = contextPath + "/quitActivity?activityId="+activityId+"&reason="+reasonMember;
            document.quitFormFuture.submit();
        }
    }

