<script type="text/javascript">
    $(document).on('click','.accordion .hd' ,function(){
        var $panel =  $(this).next(), $icon = $(this).find('.icon') ,iconPlusClass = 'icon-blue-plus', iconMinusClass = 'icon-blue-minus' ;

        if( $(this).closest('.mod-gray').length){
            iconPlusClass = 'icon-gray-plus';
            iconMinusClass = 'icon-gray-minus';
        }

        if($panel.is(':hidden')){
            $icon.removeClass(iconPlusClass).addClass(iconMinusClass);
            $panel.stop(true,true).slideDown();
        }
        else{
            $panel.stop(true,true).slideUp();
            $icon.addClass(iconPlusClass).removeClass(iconMinusClass);
        }
    })
</script>
