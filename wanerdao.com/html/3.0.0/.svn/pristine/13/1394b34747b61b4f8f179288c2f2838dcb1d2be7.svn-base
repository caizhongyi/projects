    <script type="text/javascript">
        $('.application-tabs a').click(function(){
            $(this).parent().find('a').removeClass('selected');
            $(this).addClass('selected');
            $('.mobile-app').hide().eq($(this).index()).stop(true).fadeIn();
        })
        $('.mobile-app').on('click','.prev',function(){
            var $self = $(this).closest('.mobile-app');
            var $prev = $self.prev('.mobile-app');
            if($prev.length){
                $self.hide();
                $prev.stop(true).fadeIn();
                $('.application-tabs a').removeClass('selected').eq($($prev).index()).addClass('selected');
            }
        }).on('click','.next',function(){
            var $self = $(this).closest('.mobile-app');
            var $next = $self.next('.mobile-app');
            if($next.length){
                $self.hide();
                $next.stop(true).fadeIn();
                $('.application-tabs a').removeClass('selected').eq($($next).index()).addClass('selected');
            }
        })

