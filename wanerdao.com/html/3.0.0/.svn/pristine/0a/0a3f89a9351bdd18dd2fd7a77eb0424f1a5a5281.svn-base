@charset "utf-8";img{max-width:100%;}body{font:14px/1.2 Arial,Helvetica,sans-serif;background:#fff;color:#666;}.header,.footer,.container{margin:0 auto;width:960px;}.header{background:#2d95ba;padding:10px 30px;width:900px;color:#fff;font-size:12px;font-weight:bold;line-height:25px;}.header .right{text-align:right;margin-left:10px;}.header .left{margin-right:10px;}.header h3{font-size:35px;}.header h3 span{font-weight:normal;}.header p{font-style:italic;text-align:right;}.header .company{padding:10px 5px;}.photos{text-align:center;}.photos li{float:left;overflow:hidden;margin:10px 0;}.photos li>div{width:95%;overflow:hidden;margin:0 auto;}.photos-4>li{width:25%;}.photos-2>li{width:50%;}.notes>li{margin:10px 0;border:1px solid #2d95ba;padding:15px;}.notes h3{font-size:28px;text-align:center;color:#2d95ba;}.notes .summary{margin:10px 0;border-bottom:1px dashed #2d95ba;padding-bottom:10px;color:#2d95ba;}.notes .summary span{display:inline-block;width:33%;overflow:hidden;}.notes p{text-indent:2em;line-height:25px;}.active{margin:10px 0;}.active td{border:1px solid #2d95ba;padding:5px;line-height:25px;text-align:left;}.active td.first{width:20%;color:#2d95ba;}.finance{margin:10px 0;width:100%;color:#2d95ba;line-height:30px;}.finance th{background:#f6f6f6;}.finance td{border-bottom:1px solid #ddd;}.finance tr:hover{background:#eef7fe;}.button{border:none;display:inline-block;text-align:center;cursor:pointer;overflow:hidden;}.button:hover{text-decoration:none;border-color:#89c6f5;}.blue-radius-btn,.gay-radius-btn,.disable-radius-btn{text-align:center;padding:0 30px;height:35px;line-height:35px;font-size:14px;border-radius:3px;background:url(../images/blue-gay-btn.gif) repeat-x;}a.blue-radius-btn,a.gay-radius-btn,a.disable-radius-btn{height:33px;line-height:33px;}.blue-radius-btn{border:1px solid #dadada;font-weight:bold;color:#fff;}.disable-radius-btn,.gay-radius-btn{border:1px solid #dadada;font-weight:bold;color:#fff;background-position:0 -68px;}.disable-radius-btn{cursor