.mod-gray-bordereded{
    margin: 10px 0
}

.mod-gray-borderededp {
    text-indent: 0
}


.print {
    height: 400px;
    overflow-y: auto;
    font-size: 12px
}

.print .mod-gray-bordereded{
    margin: 10px
}

.print .mod-gray-bordereded.bd {
    padding: 10px
}

.print .title, .print .content {
    line-height: 22px
}

.print .title {
    margin-bottom: 5px
}

.print .title em {
    font-weight: bold
}



.print .print-map {
    overflow: hidden
}

.print .img {
    border: 1px solid #ddd;
    padding: 1px
}

.print h3 {
    font-weight: normal;
    margin-bottom: 10px
}

.print h3 .right {
    font-size: 12px
}

.print .print-hd {
    background: #fbfbfb;
    padding: 10px;
    border-bottom: 1px solid #ddd
}

.print .photo-print {
    *zoom: 1;
    margin: 20px 20px
}

.print .photo-print:before, .print .photo-print:after {
    display: table;
    content: "";
    line-height: 0
}

.print .photo-print:after {
    clear: both
}

.print .photo-print .print-side {
    float: left;
    margin-right: 20px
}

.print .photo-print .print-main {
    overflow: hidden
}

.print .photo-print .img, .print .photo-print .img img {
    width: 177px;
    height: 233px
}

.print [class*=-list]:before,.print [class*=-list]:after{
    display: table;
    content: "";
    line-height: 0
}
.print [class*=-list]:after{clear: both}
.print [class*=-list] img{ width: 100%; height: 100%;}
/*.print [class*=-list] li { height: 165px; }*/
.print .photo-print-list {margin: 20px 20px}
.print .photo-print-list .img {margin: 10px 0}
.print .photo-print-list li { width: 100%;height: auto;}

.print .photo-col2-list, .print .photo-col4-list { *zoom: 1; margin: 20px 20px ;}
.print .photo-col2-list li, .print .photo-col4-list li {float: left;border:none;margin-bottom: 20px;  padding: 0;}
.print .photo-col2-list li .img, .print .photo-col4-list li .img{ margin: 0 10px; }
.print .photo-col2-list li{ width: 50%;}
.print .photo-col4-list li {width: 25%;}
.print .print-list { margin: 20px 20px}

.print .print-list li {
    border: 1px solid #ddd;
    padding: 10px;
    margin-bottom: 10px
}

.print .print-list li:hover {
    background: #f9f9f9
}

.print .print-ft {
    text-align: right;
    color: #999;
    padding: 10px
}

.print p {
    font-size: 12px;
    margin-bottom: 10px
}

.print-items li{ float: left; width: 50%;}