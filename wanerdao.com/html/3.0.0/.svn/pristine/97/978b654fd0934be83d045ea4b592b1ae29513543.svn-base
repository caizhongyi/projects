@keyframes rotate-round {
  0% {
    transform: rotateY(0);
  }
  100% {
    transform: rotateY(360deg);
  }
}
@-webkit-keyframes rotate-round {
  0% {
    transform: rotateY(0);
  }
  100% {
    transform: rotateY(360deg);
  }
}
.animation-rotate-round {
  animation: rotate-round 2s linear 0s infinite both;
  -webikt-animation: rotate-round 2s linear 0s infinite both;
}
@keyframes round {
  0% {
    transform: rotateY(0) translateX(0px);
  }
  25% {
    transform: rotateY(90deg) translateX(700px);
  }
  50% {
    transform: rotateY(180deg) translateX(0px);
  }
  75% {
    transform: rotateY(270deg) translateX(-700px);
  }
  100% {
    transform: rotateY(360deg) translateX(0px);
  }
}
@-webkit-keyframes round {
  0% {
    transform: rotateY(0) translateX(0px);
  }
  25% {
    transform: rotateY(90deg) translateX(700px);
  }
  50% {
    transform: rotateY(180deg) translateX(0px);
  }
  75% {
    transform: rotateY(270deg) translateX(-700px);
  }
  100% {
    transform: rotateY(360deg) translateX(0px);
  }
}
.animation-round {
  animation: round 5s linear 0s infinite both;
  -webikt-animation: round 5s linear 0s infinite both;
}
/* animation */
.fade,
.flip,
.slide,
.flip-front,
.flip-back,
.list-fade li,
.list-scale li,
.list-flip li,
.list-flip-right li,
.list-flip-bottom li {
  transition: all 0.3s ease-in-out 0s;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}
.flip-box,
.preserve3d,
.list-fade,
.list-scale,
.list-flip,
.list-flip-right,
.list-flip-bottom,
.list-hover li,
.hover {
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d;
  perspective: 800;
  -webkit-perspective: 800;
  -webkit-perspective-origin: 50% 100px;
  perspective-origin: 50% 100px;
}
.preserve3d-200 {
  perspective: 200;
  -webkit-perspective: 200;
}
.preserve3d-400 {
  perspective: 400;
  -webkit-perspective: 400;
}
.preserve3d-600 {
  perspective: 600;
  -webkit-perspective: 600;
}
.fade,
.scale,
.slide,
.list-flip li {
  visibility: hidden;
}
.in {
  visibility: inherit !important;
}
.out {
  visibility: hidden !important;
}
.list-hover li:hover .fade,
.hover:hover .fade,
.list-hover li:hover .scale,
.hover:hover .scale,
.list-hover li:hover .slide,
.hover:hover .slide {
  visibility: inherit !important;
}
.list-fade,
.list-scale,
.list-flip {
  position: relative;
}
.list-fade li,
.list-scale li,
.list-flip li,
.list-flip-right li,
.list-flip-bottom li,
.list-scale {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.list-fade .in {
  z-index: 1;
}
.fade,
.list-fade .ready {
  opacity: 0;
  height: 0;
}
.fade.in,
.list-fade .in {
  opacity: 1;
  height: auto;
}
.scale {
  -webkit-transform: scale(0);
  transform: scale(0);
}
.scale.in,
.list-hover li:hover .scale,
.hover:hover .scale {
  -webkit-transform: scale(1);
  transform: scale(1);
}
.scale-2x {
  -webkit-transform: scale(1);
  transform: scale(1);
}
.scale-2x.in,
.list-hover li:hover .scale-2x,
.hover:hover .scale-2x {
  -webkit-transform: scale(2);
  transform: scale(2);
}
.list-scale .end {
  -webkit-transform: scale(2);
  transform: scale(2);
  opacity: 0;
  z-index: 1;
}
.slide {
  -webkit-transform: translateY(-30%);
  transform: translateY(-30%);
  opacity: 0;
}
.slide.in,
.list-hover li:hover .slide,
.hover:hover .slide {
  -webkit-transform: translateY(0);
  transform: translateY(0);
  opacity: 1;
}
.slide-left {
  -webkit-transform: translateX(30%);
  transform: translateX(30%);
  opacity: 0;
}
.slide-left.in,
.list-hover li:hover .slide-left,
.hover:hover .slide-left {
  -webkit-transform: translateX(0);
  transform: translateX(0);
  opacity: 1;
}
.slide-right {
  -webkit-transform: translateX(-30%);
  transform: translateX(-30%);
  opacity: 0;
}
.slide-right.in,
.list-hover li:hover .slide-right,
.hover:hover .slide-right {
  -webkit-transform: translateX(0);
  transform: translateX(0);
  opacity: 1;
}
.slide-up {
  -webkit-transform: translateY(30%);
  transform: translateY(30%);
  opacity: 0;
}
.slide-up.in,
.list-hover li:hover .slide-up,
.hover:hover .slide-up {
  -webkit-transform: translateY(0);
  transform: translateY(0);
  opacity: 1;
}
.flip-box {
  position: relative;
  display: inline-block;
}
.flip-box .flip-front {
  position: absolute;
  left: 0;
  top: 0;
}
.flip,
.flip-front,
.flip-back {
  transition-duration: 0.7s;
}
.list-flip-right .ready,
.list-flip-bottom .ready {
  z-index: 1;
  opacity: 1;
}
.list-flip-right .in,
.list-flip-bottom .in {
  z-index: 2;
  opacity: 1;
}
.list-flip-right li,
.list-flip-bottom li {
  transition-property: transform ;
}
.flip,
.flip-right,
.list-flip li,
.flip-box:hover .flip-front,
.flip-box.in .flip-front,
.flip-box .flip-back {
  -webkit-transform: rotateY(180deg);
  transform: rotateY(180deg);
}
.flip.in,
.hover:hover .flip,
.list-hover li:hover .flip,
.list-flip .in,
.list-flip-right .in,
.flip-right.in,
.hover .flip-right,
.list-hover .flip-right,
.flip-box .flip-front,
.flip-box:hover .flip-back,
.flip-box.in .flip-back {
  -webkit-transform: rotateY(0);
  transform: rotateY(0);
}
.list-flip-right li {
  transform-origin: left  ;
  -webkit-transform-origin: left ;
}
.list-flip-bottom li {
  transform-origin: top  ;
  -webkit-transform-origin: top ;
}
.list-flip-right .end,
.list-flip-bottom .end {
  visibility: inherit !important;
}
.flip-right,
.list-flip-right .end {
  transform-origin: right  ;
  -webkit-transform-origin: right ;
}
.flip-bottom,
.list-flip-bottom .end {
  transform-origin: bottom;
  -webkit-transform-origin: bottom;
}
.list-flip-right .end,
.list-flip-right .ready {
  -webkit-transform: rotateY(90deg);
  transform: rotateY(90deg);
}
.list-flip-bottom .end,
.list-flip-bottom .ready {
  -webkit-transform: rotateX(-90deg);
  transform: rotateX(-90deg);
}
.flip-bottom {
  -webkit-transform: rotateX(-180deg);
  transform: rotateX(-180deg);
}
.flip-bottom.in,
.list-flip-bottom .in {
  -webkit-transform: rotateX(0deg);
  transform: rotateX(0deg);
}
