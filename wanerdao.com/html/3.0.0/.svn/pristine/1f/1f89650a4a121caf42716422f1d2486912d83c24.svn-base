<script type="text/javascript">
    $('.accordion .hd .icon').click(function(){
        var $panel =  $(this).parent().next();
        if($panel.is(':hidden')){
            $(this).removeClass('icon-blue-plus').addClass('icon-blue-minus');
            $panel.stop(true,true).slideDown();
        }
        else{
            $panel.stop(true,true).slideUp();
            $(this).addClass('icon-blue-plus').removeClass('icon-blue-minus');
        }
    })
</script>

