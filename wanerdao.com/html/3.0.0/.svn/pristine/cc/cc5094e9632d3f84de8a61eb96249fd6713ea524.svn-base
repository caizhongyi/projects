<!-- The main application script -->
<script  type="text/javascript">
    $(function () {
        'use strict';
        // Initialize the jQuery File Upload widget:
        $('#fileupload').fileupload({
            // Uncomment the following to send cross-domain cookies:
            //xhrFields: {withCredentials: true},
            url: 'server/php/'
        }).fileupload(
                'option',
                'redirect',
                window.location.href.replace(
                        /\/[^\/]*$/,
                        '/cors/result.html?%s'
                )
        ).fileupload('option', {
                    url: 'http://jquery-file-upload.appspot.com/',
                    maxFileSize: 5000000,
                    acceptFileTypes: /(\.|\/)(gif|jpe?g|png)$/i,
                    process: [
                        {
                            action: 'load',
                            fileTypes: /^image\/(gif|jpeg|png)$/,
                            maxFileSize: 20000000 // 20MB
                        },
                        {
                            action: 'resize',
                            maxWidth: 1440,
                            maxHeight: 900
                        },
                        {
                            action: 'save'
                        }
                    ]
                }).find(':file').on('change',function(){
                    $('.upload .upload-inner').stop(true,true).slideDown();
                })

        $('#online-upload').fileupload({
            // Uncomment the following to send cross-domain cookies:
            //xhrFields: {withCredentials: true},
            uploadTemplateId: 'template-photo-upload',
            url: 'server/php/'
        }).fileupload(
                'option',
                'redirect',
                window.location.href.replace(
                        /\/[^\/]*$/,
                        '/cors/result.html?%s'
                )
        ).fileupload('option', {
                    url: 'http://jquery-file-upload.appspot.com/',
                    maxFileSize: 5000000,
                    acceptFileTypes: /(\.|\/)(gif|jpe?g|png)$/i,
                    add: function (e, data) {
                        $('#photo-name').val(data.files[0].name);
                        data.submit();
                    },
                    done: function (e, data) {
                        data.context.text('Upload finished.');
                    },
                    process: [
                        {
                            action: 'load',
                            fileTypes: /^image\/(gif|jpeg|png)$/,
                            maxFileSize: 20000000 // 20MB
                        },
                        {
                            action: 'resize',
                            maxWidth: 1440,
                            maxHeight: 900
                        },
                        {
                            action: 'save'
                        }
                    ]
                }).find('#add-photo').on('click',function(){
                    var inner = $('.upload-inner',$('#online-upload'));
                    var $name = $('#photo-name'),
                        $descript = $('#photo-descript');
                    if($name.val() == ""){
                        alert('请选择Upload图片！');
                        return ;
                    }
                    inner.stop(true,true).slideDown();
                    var html = '<li><span class="name">'+ $name.val() +'</span><span class="descript">'+ $descript.val() +'</span><a href="javascript:;" class="gray-del cancel"></li>';
                    inner.append(html);
                    $name.val('');
                    $descript.val('');
                })
        $('#online-upload').on('click','.gray-del',function(){
            var li = $(this).closest('li'),
                index = li.index();
            li.parent().next().children('li:eq('+ index +')').find('.cancle').click();
            $(this).closest('li').remove();
        });
    });
</script>
