<!-- The template to display files available for upload -->
<script id="template-upload" type="text/x-tmpl">
    {% for (var i=0, file; file=o.files[i]; i++) { %}
    <li class="template-upload fade">
        <span class="preview"><span class="fade"></span></span>
        <span class="name"><span>{%=file.name%}</span></span>
        <span class="size"><span>{%=o.formatFileSize(file.size)%}</span></span>
        {% if (file.error) { %}
        <span class="error" colspan="2"><span class="label label-important">Error</span> {%=file.error%}</span>
        {% } else if (o.files.valid && !i) { %}
        <span>
            <div class="progress progress-success progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0"><div class="bar" style="width:0%;"></div></div>
        </span>
        <span class="start">{% if (!o.options.autoUpload) { %}
           <!-- <button></button>-->
            {% } %}</span>
        {% } else { %}
        <span colspan="2"></span>
        {% } %}
        <span class="cancel">{% if (!i) { %}
            <button class="gray-del"></button>
            {% } %}</span>
    </li>
    {% } %}
</script>
